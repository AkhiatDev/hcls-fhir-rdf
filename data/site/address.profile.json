                "exampleCode": "both",
                "type": [
                    {
                        "code": "code"
                    }
                ],
                "mapping": [
                    {
                        "identity": "vcard",
                        "map": "address type parameter"
                    },
                    {
                        "identity": "v2",
                        "map": "XAD.18"
                    },
                    {
                        "identity": "rim",
                        "map": "unique(./use)"
                    }
                ],
                "short": "postal | physical | both",
                "isSummary": true,
                "binding": {
                    "strength": "required",
                    "description": "The type of an address (physical / postal)",
                    "valueSetReference": {
                        "reference": "http://hl7.org/fhir/ValueSet/address-type"
                    }
                }
            },
            {
                "path": "Address.text",
                "max": "1",
