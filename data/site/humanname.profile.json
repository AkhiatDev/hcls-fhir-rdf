                    },
                    {
                        "identity": "rim",
                        "map": "unique(./use)"
                    },
                    {
                        "identity": "servd",
                        "map": "./NamePurpose"
                    }
                ],
                "requirements": "Allows the appropriate name for a particular context of use to be selected from among a set of names.",
                "isModifier": true
            },
