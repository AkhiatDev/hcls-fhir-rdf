[] a :Alert;
      Alert:text [
         a :Narrative;
         Narrative:status [
            a fhir:code;
            fhir:value "generated";
         ];
         Narrative:div [
            a fhir:xhtml;
            fhir:text """Large Dog warning""";
         ]
      ];
      Alert:category [
         a :CodeableConcept;
         CodeableConcept:coding [
            a :Coding;
            Coding:system [
               a fhir:uri;
               fhir:value "local";
            ];
            Coding:code [
               a fhir:code;
               fhir:value "admin";
            ];
            Coding:display [
               a fhir:string;
               fhir:value "Admin";
            ]
         ];
         CodeableConcept:text [
            a fhir:string;
            fhir:value "admin";
         ];
      ];
      Alert:status [
         a fhir:code;
         fhir:value "active";
      ];
      Alert:subject [
         a fhir:Resource;
         a fhir:PatientReference;
         Reference:reference <patient/@example>;
         Reference:display [
            a fhir:String; fhir:value "Peter Patient"
         ]
      ];
      Alert:author [
         a fhir:Resource;
         a fhir:PractitionerReference;
         Reference:reference <practitioner/@example>;
         Reference:display [
            a fhir:String; fhir:value "Nancy Nurse"
         ]
      ];
      Alert:note [
         a fhir:string;
         fhir:value "patient has a big dog at his home. Always carry a slab of meat to pacify it";
      ]
.