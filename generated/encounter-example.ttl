[] a :Encounter;
      Encounter:text [
         a :Narrative;
         Narrative:status [
            a fhir:code;
            fhir:value "generated";
         ];
         Narrative:div [
            a fhir:xhtml;
            fhir:text """<div>Encounter with patient @example</div>Encounter with patient @example""";
         ]
      ];
      Encounter:status [
         a fhir:code;
         fhir:value "current";
      ];
      Encounter:class [
         a fhir:code;
         fhir:value "inpatient";
      ];
      Encounter:subject [
         a fhir:Resource;
         a fhir:PatientReference;
         Reference:reference <patient/@example>;
      ];
.