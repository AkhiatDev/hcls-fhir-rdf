@prefix : <http://hl7/org/fhir/> .
@prefix xml: <http://www.w3.org/1999/xhtml> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema> .
@base <http://this-fhir-server/fhir/> .

[] a :Order;
  :resource_text [
    a :Narrative;
    :narrative_status [a :Code; :value "generated"];
    xml:div """Request for Prescription (on patient Donald DUCK @ Acme Healthcare, Inc. MR = 654321)"""
  ];
  :order_date [
    a :DateTime;
    :value "2012-12-28T09:03:04+11:00"^^xsd:dateTime
  ];
  :order_subject [
    a :Patient;
    :reference <patient/@pat2>
  ];
  :order_source [
    a :Practitioner;
    :reference <practitioner/@example>
  ];
  :order_reason [
    a :String;
    :value "Standard admission testing"
  ];
  :order_when [
    :order_when_code [
      a :CodeableConcept;
      :codeableconcept_coding [
	a :Coding;
	:coding_system [a :Uri; :value <http://acme.com/codes/request-priority>];
	:coding_code  [a :Code; :value "today"];
      ]
    ]
  ];
  :order_detail [
    a :MedicationPrescription;
    :reference <medicationprescription/@example>
  ].