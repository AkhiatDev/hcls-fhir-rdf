@prefix : <http://example.org/Patient/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fhir: <http://hl7.org/fhir/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.org/Patient/> .

<http://example.org/Patient/PeterPatient> rdf:type owl:Ontology ;
                              
                              owl:imports <http://hl7.org/fhir> .

###  http://example.org/Patient/PeterPatient

:PeterPatient rdf:type fhir:Patient , fhir:DomainResource ,
                       owl:NamedIndividual ;
              
              rdfs:label "Peter Patient" ;
              
              fhir:Patient.name [ fhir:HumanName.family [ fhir:value "Patient" ] ;
                                  fhir:HumanName.given [ fhir:value "Peter"]
              ] .


