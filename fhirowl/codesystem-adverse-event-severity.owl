@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix adverse-event-severity: <http://hl7.org/fhir/adverse-event-severity/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

adverse-event-severity:Severe
        a           owl:Class ;
        rdfs:label  "Severe" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Severe" .

adverse-event-severity:Mild
        a           owl:Class ;
        rdfs:label  "Mild" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Mild" .

adverse-event-severity:Moderate
        a           owl:Class ;
        rdfs:label  "Moderate" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Moderate" .

adverse-event-severity:
        a                owl:Ontology ;
        rdfs:comment     "The severity of the adverse event itself, in direct relation to the subject" ;
        rdfs:label       "AdverseEventSeverity" ;
        owl:versionIRI   "http://hl7.org/fhir/adverse-event-severity/3.1.0" ;
        owl:versionInfo  "AdverseEventSeverity(3.1.0)" .
