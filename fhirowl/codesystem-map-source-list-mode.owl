@prefix map-source-list-mode: <http://hl7.org/fhir/map-source-list-mode/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

map-source-list-mode:last
        a           owl:Class ;
        rdfs:label  "Last" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Only process this rule for the last in the list" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Last" .

map-source-list-mode:first
        a           owl:Class ;
        rdfs:label  "First" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Only process this rule for the first in the list" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "First" .

map-source-list-mode:not_first
        a           owl:Class ;
        rdfs:label  "All but the first" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Process this rule for all but the first" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "All but the first" .

map-source-list-mode:only_one
        a           owl:Class ;
        rdfs:label  "Enforce only one" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Only process this rule is there is only item" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Enforce only one" .

map-source-list-mode:not_last
        a           owl:Class ;
        rdfs:label  "All but the last" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Process this rule for all but the last" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "All but the last" .

map-source-list-mode:
        a                owl:Ontology ;
        rdfs:comment     "If field is a list, how to manage the source" ;
        rdfs:label       "StructureMapSourceListMode" ;
        owl:versionIRI   "http://hl7.org/fhir/map-source-list-mode/3.1.0" ;
        owl:versionInfo  "StructureMapSourceListMode(3.1.0)" .
