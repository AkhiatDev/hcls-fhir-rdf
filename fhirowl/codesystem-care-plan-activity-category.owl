@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix care-plan-activity-category: <http://hl7.org/fhir/care-plan-activity-category/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

care-plan-activity-category:diet
        a           owl:Class ;
        rdfs:label  "Diet" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Plan for the patient to consume food of a specified nature" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Diet" .

care-plan-activity-category:other
        a           owl:Class ;
        rdfs:label  "Other" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Some other form of action" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Other" .

care-plan-activity-category:observation
        a           owl:Class ;
        rdfs:label  "Observation" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Plan to capture information about a patient (vitals, labs, diagnostic images, etc.)" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Observation" .

care-plan-activity-category:drug
        a           owl:Class ;
        rdfs:label  "Drug" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Plan for the patient to consume/receive a drug, vaccine or other product" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Drug" .

care-plan-activity-category:
        a                owl:Ontology ;
        rdfs:comment     "High-level categorization of the type of activity in a care plan." ;
        rdfs:label       "CarePlanActivityCategory" ;
        owl:versionIRI   "http://hl7.org/fhir/care-plan-activity-category/1.0.0" ;
        owl:versionInfo  "CarePlanActivityCategory(1.0.0)" .

care-plan-activity-category:encounter
        a           owl:Class ;
        rdfs:label  "Encounter" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Plan to meet or communicate with the patient (in-patient, out-patient, phone call, etc.)" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Encounter" .

care-plan-activity-category:supply
        a           owl:Class ;
        rdfs:label  "Supply" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Plan to provide something to the patient (medication, medical supply, etc.)" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Supply" .

care-plan-activity-category:procedure
        a           owl:Class ;
        rdfs:label  "Procedure" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Plan to modify the patient in some way (surgery, physiotherapy, education, counseling, etc.)" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Procedure" .
