@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix assert-operator-codes: <http://hl7.org/fhir/assert-operator-codes/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

assert-operator-codes:lessThan
        a           owl:Class ;
        rdfs:label  "lessThan" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Compare value to be less than a known value." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "lessThan" .

assert-operator-codes:equals
        a           owl:Class ;
        rdfs:label  "equals" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Default value. Equals comparison." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "equals" .

assert-operator-codes:eval
        a           owl:Class ;
        rdfs:label  "evaluate" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Evaluate the fluentpath expression as a boolean condition." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "evaluate" .

assert-operator-codes:in
        a           owl:Class ;
        rdfs:label  "in" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Compare value within a known set of values." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "in" .

assert-operator-codes:notIn
        a           owl:Class ;
        rdfs:label  "notIn" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Compare value not within a known set of values." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "notIn" .

assert-operator-codes:notEquals
        a           owl:Class ;
        rdfs:label  "notEquals" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Not equals comparison." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "notEquals" .

assert-operator-codes:greaterThan
        a           owl:Class ;
        rdfs:label  "greaterThan" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Compare value to be greater than a known value." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "greaterThan" .

assert-operator-codes:contains
        a           owl:Class ;
        rdfs:label  "contains" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Compare value string contains a known value." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "contains" .

assert-operator-codes:
        a                owl:Ontology ;
        rdfs:comment     "The type of operator to use for assertion." ;
        rdfs:label       "AssertionOperatorType" ;
        owl:versionIRI   "http://hl7.org/fhir/assert-operator-codes/3.1.0" ;
        owl:versionInfo  "AssertionOperatorType(3.1.0)" .

assert-operator-codes:notEmpty
        a           owl:Class ;
        rdfs:label  "notEmpty" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Compare value is not empty." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "notEmpty" .

assert-operator-codes:empty
        a           owl:Class ;
        rdfs:label  "empty" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Compare value is empty." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "empty" .

assert-operator-codes:notContains
        a           owl:Class ;
        rdfs:label  "notContains" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Compare value string does not contain a known value." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "notContains" .
