@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix practitioner-role: <http://hl7.org/fhir/practitioner-role/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

practitioner-role:pharmacist
        a           owl:Class ;
        rdfs:label  "Pharmacist" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A qualified/registered/licensed pharmacist" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Pharmacist" .

practitioner-role:doctor
        a           owl:Class ;
        rdfs:label  "Doctor" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A qualified/registered medical practitioner" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Doctor" .

practitioner-role:teacher
        a           owl:Class ;
        rdfs:label  "Teacher/educator" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Someone who is able to provide educational services" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Teacher/educator" .

practitioner-role:ict
        a           owl:Class ;
        rdfs:label  "ICT professional" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Someone who is qualified in Information and Communication Technologies" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "ICT professional" .

practitioner-role:researcher
        a           owl:Class ;
        rdfs:label  "Researcher" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A practitioner that may perform research" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Researcher" .

practitioner-role:nurse
        a           owl:Class ;
        rdfs:label  "Nurse" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A practitoner with nursing experience that may be qualified/registered" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Nurse" .

practitioner-role:  a    owl:Ontology ;
        rdfs:comment     "This example value set defines a set of codes that can be used to indicate the role of a Practitioner." ;
        rdfs:label       "PractitionerRole" ;
        owl:versionIRI   "http://hl7.org/fhir/practitioner-role/3.1.0" ;
        owl:versionInfo  "PractitionerRole(3.1.0)" .
