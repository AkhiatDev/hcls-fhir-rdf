@prefix document-relationship-type: <http://hl7.org/fhir/document-relationship-type/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

document-relationship-type:replaces
        a           owl:Class ;
        rdfs:label  "Replaces" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This document logically replaces or supersedes the target document." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Replaces" .

document-relationship-type:
        a                owl:Ontology ;
        rdfs:comment     "The type of relationship between documents." ;
        rdfs:label       "DocumentRelationshipType" ;
        owl:versionIRI   "http://hl7.org/fhir/document-relationship-type/3.1.0" ;
        owl:versionInfo  "DocumentRelationshipType(3.1.0)" .

document-relationship-type:signs
        a           owl:Class ;
        rdfs:label  "Signs" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This document is a signature of the target document." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Signs" .

document-relationship-type:appends
        a           owl:Class ;
        rdfs:label  "Appends" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This document adds additional information to the target document." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Appends" .

document-relationship-type:transforms
        a           owl:Class ;
        rdfs:label  "Transforms" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This document was generated by transforming the target document (e.g. format or language conversion)." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Transforms" .
