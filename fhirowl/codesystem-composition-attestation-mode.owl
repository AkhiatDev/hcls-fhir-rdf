@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix composition-attestation-mode: <http://hl7.org/fhir/composition-attestation-mode/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

composition-attestation-mode:professional
        a           owl:Class ;
        rdfs:label  "Professional" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The person authenticated the content in their professional capacity." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Professional" .

composition-attestation-mode:
        a                owl:Ontology ;
        rdfs:comment     "The way in which a person authenticated a composition." ;
        rdfs:label       "CompositionAttestationMode" ;
        owl:versionIRI   "http://hl7.org/fhir/composition-attestation-mode/3.1.0" ;
        owl:versionInfo  "CompositionAttestationMode(3.1.0)" .

composition-attestation-mode:official
        a           owl:Class ;
        rdfs:label  "Official" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The organization authenticated the content as consistent with their policies and procedures." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Official" .

composition-attestation-mode:personal
        a           owl:Class ;
        rdfs:label  "Personal" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The person authenticated the content in their personal capacity." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Personal" .

composition-attestation-mode:legal
        a           owl:Class ;
        rdfs:label  "Legal" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The person authenticated the content and accepted legal responsibility for its content." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Legal" .
