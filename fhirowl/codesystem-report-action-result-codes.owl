@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix report-action-result-codes: <http://hl7.org/fhir/report-action-result-codes/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

report-action-result-codes:error
        a           owl:Class ;
        rdfs:label  "Error" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The action encountered a fatal error and the engine was unable to process." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Error" .

report-action-result-codes:skip
        a           owl:Class ;
        rdfs:label  "Skip" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The action was skipped." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Skip" .

report-action-result-codes:warning
        a           owl:Class ;
        rdfs:label  "Warning" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The action passed but with warnings." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Warning" .

report-action-result-codes:fail
        a           owl:Class ;
        rdfs:label  "Fail" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The action failed." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Fail" .

report-action-result-codes:pass
        a           owl:Class ;
        rdfs:label  "Pass" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The action was successful." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Pass" .

report-action-result-codes:
        a                owl:Ontology ;
        rdfs:comment     "The results of executing an action." ;
        rdfs:label       "TestReportActionResult" ;
        owl:versionIRI   "http://hl7.org/fhir/report-action-result-codes/3.1.0" ;
        owl:versionInfo  "TestReportActionResult(3.1.0)" .
