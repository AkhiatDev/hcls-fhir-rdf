@prefix message-events: <http://hl7.org/fhir/message-events/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

message-events:MedicationAdministration-Update
        a           owl:Class ;
        rdfs:label  "MedicationAdministration Update" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Update a Medication Administration record." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "MedicationAdministration Update" .

message-events:patient-link
        a           owl:Class ;
        rdfs:label  "Patient Link" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Notification that two patient records actually identify the same patient." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Patient Link" .

message-events:admin-notify
        a           owl:Class ;
        rdfs:label  "Admin Notify" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Notification of a change to an administrative resource (either create or update). Note that there is no delete, though some administrative resources have status or period elements for this use." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Admin Notify" .

message-events:communication-request
        a           owl:Class ;
        rdfs:label  "Communication Request" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Notification to convey information." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Communication Request" .

message-events:patient-unlink
        a           owl:Class ;
        rdfs:label  "Patient Unlink" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Notification that previous advice that two patient records concern the same patient is now considered incorrect." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Patient Unlink" .

message-events:diagnosticreport-provide
        a           owl:Class ;
        rdfs:label  "Diagnosticreport Provide" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Provide a diagnostic report, or update a previously provided diagnostic report." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Diagnosticreport Provide" .

message-events:  a       owl:Ontology ;
        rdfs:comment     "One of the message events defined as part of FHIR." ;
        rdfs:label       "MessageEvent" ;
        owl:versionIRI   "http://hl7.org/fhir/message-events/3.1.0" ;
        owl:versionInfo  "MessageEvent(3.1.0)" .

message-events:valueset-expand
        a           owl:Class ;
        rdfs:label  "Valueset Expand" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The definition of a value set is used to create a simple collection of codes suitable for use for data entry or validation. An expanded value set will be returned, or an error message." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Valueset Expand" .

message-events:MedicationAdministration-Recording
        a           owl:Class ;
        rdfs:label  "MedicationAdministration Recording" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Indicates that a medication has been recorded against the patient's record." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "MedicationAdministration Recording" .

message-events:MedicationAdministration-Complete
        a           owl:Class ;
        rdfs:label  "MedicationAdministration Complete" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Change the status of a Medication Administration to show that it is complete." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "MedicationAdministration Complete" .

message-events:MedicationAdministration-Nullification
        a           owl:Class ;
        rdfs:label  "MedicationAdministration Nullification" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Someone wishes to record that the record of administration of a medication is in error and should be ignored." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "MedicationAdministration Nullification" .

message-events:codesystem-expand
        a           owl:Class ;
        rdfs:label  "Codesystem Expand" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The definition of a code system is used to create a simple collection of codes suitable for use for data entry or validation. An expanded code system will be returned, or an error message." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Codesystem Expand" .

message-events:observation-provide
        a           owl:Class ;
        rdfs:label  "Observation Provide" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Provide a simple observation or update a previously provided simple observation." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Observation Provide" .
