@prefix concept-property-type: <http://hl7.org/fhir/concept-property-type/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

concept-property-type:code
        a           owl:Class ;
        rdfs:label  "code (internal reference)" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The property value is a code that identifies a concept defined in the code system" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "code (internal reference)" .

concept-property-type:Coding
        a           owl:Class ;
        rdfs:label  "Coding (external reference)" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The property  value is a code defined in an external code system. This may be used for translations, but is not the intent" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Coding (external reference)" .

concept-property-type:boolean
        a           owl:Class ;
        rdfs:label  "boolean" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The property value is a boolean true | false" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "boolean" .

concept-property-type:integer
        a           owl:Class ;
        rdfs:label  "integer" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The property value is a string (often used to assign ranking values to concepts for supporting score assessments)" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "integer" .

concept-property-type:dateTime
        a           owl:Class ;
        rdfs:label  "dateTime" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The property is a date or a date + time" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "dateTime" .

concept-property-type:string
        a           owl:Class ;
        rdfs:label  "string" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The property value is a string" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "string" .

concept-property-type:
        a                owl:Ontology ;
        rdfs:comment     "The type of a property value" ;
        rdfs:label       "PropertyType" ;
        owl:versionIRI   "http://hl7.org/fhir/concept-property-type/3.1.0" ;
        owl:versionInfo  "PropertyType(3.1.0)" .
