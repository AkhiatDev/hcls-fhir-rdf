@prefix supplydelivery-type: <http://hl7.org/fhir/supply-item-type/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

supplydelivery-type:  a  owl:Ontology ;
        rdfs:comment     "This value sets refers to a specific supply item." ;
        rdfs:label       "Supply Item Type" ;
        owl:versionIRI   "http://hl7.org/fhir/supply-item-type/3.1.0" ;
        owl:versionInfo  "Supply Item Type(3.1.0)" .

supplydelivery-type:medication
        a           owl:Class ;
        rdfs:label  "Medication" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Supply is a kind of medication." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Medication" .

supplydelivery-type:device
        a           owl:Class ;
        rdfs:label  "Device" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "What is supplied (or requested) is a device." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Device" .
