@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix restful-security-service: <http://hl7.org/fhir/restful-security-service/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

restful-security-service:SMART-on-FHIR
        a           owl:Class ;
        rdfs:label  "SMART-on-FHIR" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "OAuth2 using SMART-on-FHIR profile (see http://docs.smarthealthit.org/)." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "SMART-on-FHIR" .

restful-security-service:OAuth
        a           owl:Class ;
        rdfs:label  "OAuth" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Oauth (unspecified version see oauth.net)." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "OAuth" .

restful-security-service:Basic
        a           owl:Class ;
        rdfs:label  "Basic" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Basic authentication defined in HTTP specification." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Basic" .

restful-security-service:Kerberos
        a           owl:Class ;
        rdfs:label  "Kerberos" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "see http://www.ietf.org/rfc/rfc4120.txt." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Kerberos" .

restful-security-service:NTLM
        a           owl:Class ;
        rdfs:label  "NTLM" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Microsoft NTLM Authentication." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "NTLM" .

restful-security-service:Certificates
        a           owl:Class ;
        rdfs:label  "Certificates" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "SSL where client must have a certificate registered with the server." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Certificates" .

restful-security-service:
        a                owl:Ontology ;
        rdfs:comment     "Types of security services used with FHIR." ;
        rdfs:label       "RestfulSecurityService" ;
        owl:versionIRI   "http://hl7.org/fhir/restful-security-service/3.1.0" ;
        owl:versionInfo  "RestfulSecurityService(3.1.0)" .
