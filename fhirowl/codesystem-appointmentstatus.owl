@prefix appointmentstatus: <http://hl7.org/fhir/appointmentstatus/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

appointmentstatus:pending
        a           owl:Class ;
        rdfs:label  "Pending" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Some or all of the participant(s) have not finalized their acceptance of the appointment request." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Pending" .

appointmentstatus:cancelled
        a           owl:Class ;
        rdfs:label  "Cancelled" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The appointment has been cancelled." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Cancelled" .

appointmentstatus:fulfilled
        a           owl:Class ;
        rdfs:label  "Fulfilled" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This appointment has completed and may have resulted in an encounter." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Fulfilled" .

appointmentstatus:noshow
        a           owl:Class ;
        rdfs:label  "No Show" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Some or all of the participant(s) have not/did not appear for the appointment (usually the patient)." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "No Show" .

appointmentstatus:proposed
        a           owl:Class ;
        rdfs:label  "Proposed" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "None of the participant(s) have finalized their acceptance of the appointment request, and the start/end time may not be set yet." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Proposed" .

appointmentstatus:arrived
        a           owl:Class ;
        rdfs:label  "Arrived" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Some of the patients have arrived." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Arrived" .

appointmentstatus:  a    owl:Ontology ;
        rdfs:comment     "The free/busy status of an appointment." ;
        rdfs:label       "AppointmentStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/appointmentstatus/3.1.0" ;
        owl:versionInfo  "AppointmentStatus(3.1.0)" .

appointmentstatus:entered-in-error
        a           owl:Class ;
        rdfs:label  "Entered in error" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This instance should not have been part of this patient's medical record." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Entered in error" .

appointmentstatus:booked
        a           owl:Class ;
        rdfs:label  "Booked" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "All participant(s) have been considered and the appointment is confirmed to go ahead at the date/times specified." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Booked" .
