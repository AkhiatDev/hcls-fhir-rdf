@prefix fm-status: <http://hl7.org/fhir/fm-status/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

fm-status:active  a  owl:Class ;
        rdfs:label  "Active" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The instance is currently in-force." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Active" .

fm-status:draft  a  owl:Class ;
        rdfs:label  "Draft" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A new instance the contents of which is not complete." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Draft" .

fm-status:entered-in-error
        a           owl:Class ;
        rdfs:label  "Entered in Error" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The instance was entered in error." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Entered in Error" .

fm-status:  a            owl:Ontology ;
        rdfs:comment     "This value set includes Status codes." ;
        rdfs:label       "Financial Resource Status Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/fm-status/3.1.0" ;
        owl:versionInfo  "Financial Resource Status Codes(3.1.0)" .

fm-status:cancelled  a  owl:Class ;
        rdfs:label  "Cancelled" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The instance is withdrawn, rescinded or reversed." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Cancelled" .
