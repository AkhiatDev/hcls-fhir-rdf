@prefix structure-definition-kind: <http://hl7.org/fhir/structure-definition-kind/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

structure-definition-kind:logical
        a           owl:Class ;
        rdfs:label  "Logical Model" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A conceptual package of data that will be mapped to resources for implementation." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Logical Model" .

structure-definition-kind:complex-type
        a           owl:Class ;
        rdfs:label  "Complex Data Type" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A  complex structure that defines a set of data elements. These can be used throughout Resource and extension definitions, and in logical models." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Complex Data Type" .

structure-definition-kind:resource
        a           owl:Class ;
        rdfs:label  "Resource" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A resource defined by the FHIR specification." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Resource" .

structure-definition-kind:primitive-type
        a           owl:Class ;
        rdfs:label  "Primitive Data Type" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A primitive type that has a value and an extension. These can be used throughout Resource and extension definitions. Only the base specification can define primitive types." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Primitive Data Type" .

structure-definition-kind:
        a                owl:Ontology ;
        rdfs:comment     "Defines the type of structure that a definition is describing." ;
        rdfs:label       "StructureDefinitionKind" ;
        owl:versionIRI   "http://hl7.org/fhir/structure-definition-kind/3.1.0" ;
        owl:versionInfo  "StructureDefinitionKind(3.1.0)" .
