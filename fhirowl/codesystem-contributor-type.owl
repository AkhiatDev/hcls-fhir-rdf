@prefix fhir:  <http://hl7.org/fhir/> .
@prefix contributor-type: <http://hl7.org/fhir/contributor-type/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

contributor-type:  a     owl:Ontology ;
        rdfs:comment     "The type of contributor" ;
        rdfs:label       "ContributorType" ;
        owl:versionIRI   "http://hl7.org/fhir/contributor-type/3.1.0" ;
        owl:versionInfo  "ContributorType(3.1.0)" .

contributor-type:reviewer
        a           owl:Class ;
        rdfs:label  "Reviewer" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A reviewer of the content of the module" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Reviewer" .

contributor-type:editor
        a           owl:Class ;
        rdfs:label  "Editor" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "An editor of the content of the module" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Editor" .

contributor-type:endorser
        a           owl:Class ;
        rdfs:label  "Endorser" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "An endorser of the content of the module" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Endorser" .

contributor-type:author
        a           owl:Class ;
        rdfs:label  "Author" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "An author of the content of the module" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Author" .
