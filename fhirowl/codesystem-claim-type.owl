@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix claim-type: <http://hl7.org/fhir/ex-claimtype/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

claim-type:  a           owl:Ontology ;
        rdfs:comment     "This value set includes sample Claim Type codes." ;
        rdfs:label       "Example Claim Type Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/ex-claimtype/3.1.0" ;
        owl:versionInfo  "Example Claim Type Codes(3.1.0)" .

claim-type:oral  a  owl:Class ;
        rdfs:label  "Oral" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Dental, Denture and Hygiene claims." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Oral" .

claim-type:pharmacy  a  owl:Class ;
        rdfs:label  "Pharmacy" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Pharmacy claims for goods and services." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Pharmacy" .

claim-type:professional
        a           owl:Class ;
        rdfs:label  "Professional" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Typically outpatient claims from Physician, Psychological, Chiropractor, Physiotherapy, Speech Pathology, rehabilitative, consulting." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Professional" .

claim-type:vision  a  owl:Class ;
        rdfs:label  "Vision" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Vision claims for professional services and products such as glasses and contact lenses." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Vision" .

claim-type:institutional
        a           owl:Class ;
        rdfs:label  "Institutional" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Hospital, clinic and typically inpatient claims." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Institutional" .
