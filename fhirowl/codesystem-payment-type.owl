@prefix payment-type: <http://hl7.org/fhir/payment-type/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

payment-type:advance  a  owl:Class ;
        rdfs:label  "Advance" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The amount is an advance against future claims." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Advance" .

payment-type:  a         owl:Ontology ;
        rdfs:comment     "This value set includes sample Payment Type codes." ;
        rdfs:label       "Payment Type Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/payment-type/3.1.0" ;
        owl:versionInfo  "Payment Type Codes(3.1.0)" .

payment-type:adjustment
        a           owl:Class ;
        rdfs:label  "Adjustment" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The amount is an adjustment regarding claims already paid." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Adjustment" .

payment-type:payment  a  owl:Class ;
        rdfs:label  "Payment" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The amount is partial or complete settlement of the amounts due." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Payment" .
