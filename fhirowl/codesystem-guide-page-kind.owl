@prefix fhir:  <http://hl7.org/fhir/> .
@prefix guide-page-kind: <http://hl7.org/fhir/guide-page-kind/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

guide-page-kind:resource
        a           owl:Class ;
        rdfs:label  "Resource" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This is a page that represents a presented resource. This is typically used for generated conformance resource presentations." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Resource" .

guide-page-kind:list  a  owl:Class ;
        rdfs:label  "List" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This is a page that represents a list of resources of one or more types." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "List" .

guide-page-kind:page  a  owl:Class ;
        rdfs:label  "Page" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This is a page of content that is included in the implementation guide. It has no particular function." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Page" .

guide-page-kind:directory
        a           owl:Class ;
        rdfs:label  "Directory" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This is a page that lists the resources of a given type, and also creates pages for all the listed types as other pages in the section." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Directory" .

guide-page-kind:include
        a           owl:Class ;
        rdfs:label  "Include" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This is a page showing where an included guide is injected." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Include" .

guide-page-kind:toc  a  owl:Class ;
        rdfs:label  "Table Of Contents" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This is a generated page that contains the table of contents." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Table Of Contents" .

guide-page-kind:  a      owl:Ontology ;
        rdfs:comment     "The kind of an included page." ;
        rdfs:label       "GuidePageKind" ;
        owl:versionIRI   "http://hl7.org/fhir/guide-page-kind/3.1.0" ;
        owl:versionInfo  "GuidePageKind(3.1.0)" .

guide-page-kind:dictionary
        a                owl:Class ;
        rdfs:label       "Dictionary" ;
        rdfs:subClassOf  guide-page-kind:directory ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This is a page that creates the listed resources as a dictionary." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Dictionary" .

guide-page-kind:example
        a           owl:Class ;
        rdfs:label  "Example" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This is a page that represents a human readable rendering of an example." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Example" .
