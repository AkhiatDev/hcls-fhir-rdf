@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix goal-acceptance-status: <http://hl7.org/fhir/goal-acceptance-status/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

goal-acceptance-status:agree
        a           owl:Class ;
        rdfs:label  "Agree" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Stakeholder supports pursuit of the goal" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Agree" .

goal-acceptance-status:disagree
        a           owl:Class ;
        rdfs:label  "Disagree" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Stakeholder is not in support of the pursuit of the goal" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Disagree" .

goal-acceptance-status:
        a                owl:Ontology ;
        rdfs:comment     "Codes indicating whether the goal has been accepted by a stakeholder" ;
        rdfs:label       "GoalAcceptanceStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/goal-acceptance-status/3.1.0" ;
        owl:versionInfo  "GoalAcceptanceStatus(3.1.0)" .

goal-acceptance-status:pending
        a           owl:Class ;
        rdfs:label  "Pending" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Stakeholder has not yet made a decision on whether they support the goal" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Pending" .
