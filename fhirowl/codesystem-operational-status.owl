@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix operational-status: <http://hl7.org/fhir/operational-status/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

operational-status:on
        a           owl:Class ;
        rdfs:label  "On" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The device is fully operational." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "On" .

operational-status:hw-discon
        a           owl:Class ;
        rdfs:label  "Hardware Disconnectd" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The device hardware is disconnected." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Hardware Disconnectd" .

operational-status:entered-in-error
        a           owl:Class ;
        rdfs:label  "Entered In Error" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The device was entered in error." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Entered In Error" .

operational-status:  a   owl:Ontology ;
        rdfs:comment     "Codes representing the current status of the device - on, off, suspended, etc." ;
        rdfs:label       "DeviceComponentOperationalStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/operational-status/3.1.0" ;
        owl:versionInfo  "DeviceComponentOperationalStatus(3.1.0)" .

operational-status:standby
        a           owl:Class ;
        rdfs:label  "Standby" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The device is ready but not actively operating." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Standby" .

operational-status:not-ready
        a           owl:Class ;
        rdfs:label  "Not Ready" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The device is not ready." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Not Ready" .

operational-status:off
        a           owl:Class ;
        rdfs:label  "Off" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The device is off." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Off" .

operational-status:transduc-discon
        a           owl:Class ;
        rdfs:label  "Transducer Diconnected" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The device transducer is diconnected." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Transducer Diconnected" .
