@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .
@prefix explanationofbenefit-status: <http://hl7.org/fhir/explanationofbenefit-status/> .

explanationofbenefit-status:entered-in-error
        a           owl:Class ;
        rdfs:label  "Entered In Error" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The resource instance was entered in error." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Entered In Error" .

explanationofbenefit-status:draft
        a           owl:Class ;
        rdfs:label  "Draft" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A new resource instance the contents of which is not complete." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Draft" .

explanationofbenefit-status:
        a                owl:Ontology ;
        rdfs:comment     "A code specifying the state of the resource instance." ;
        rdfs:label       "ExplanationOfBenefitStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/explanationofbenefit-status/3.1.0" ;
        owl:versionInfo  "ExplanationOfBenefitStatus(3.1.0)" .

explanationofbenefit-status:active
        a           owl:Class ;
        rdfs:label  "Active" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The resource instance is currently in-force." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Active" .

explanationofbenefit-status:cancelled
        a           owl:Class ;
        rdfs:label  "Cancelled" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The resource instance is withdrawn, rescinded or reversed." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Cancelled" .
