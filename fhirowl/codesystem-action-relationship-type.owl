@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix action-relationship-type: <http://hl7.org/fhir/action-relationship-type/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

action-relationship-type:before-end
        a           owl:Class ;
        rdfs:label  "Before End" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The action must be performed before the end of the related action" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Before End" .

action-relationship-type:after-end
        a           owl:Class ;
        rdfs:label  "After End" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The action must be performed after the end of the related action" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "After End" .

action-relationship-type:after
        a           owl:Class ;
        rdfs:label  "After" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The action must be performed after the related action" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "After" .

action-relationship-type:after-start
        a           owl:Class ;
        rdfs:label  "After Start" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The action must be performed after the start of the related action" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "After Start" .

action-relationship-type:before-start
        a           owl:Class ;
        rdfs:label  "Before Start" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The action must be performed before the start of the related action" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Before Start" .

action-relationship-type:
        a                owl:Ontology ;
        rdfs:comment     "Defines the types of relationships between actions" ;
        rdfs:label       "ActionRelationshipType" ;
        owl:versionIRI   "http://hl7.org/fhir/action-relationship-type/3.1.0" ;
        owl:versionInfo  "ActionRelationshipType(3.1.0)" .

action-relationship-type:concurrent-with-start
        a           owl:Class ;
        rdfs:label  "Concurrent With Start" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The action must be performed concurrent with the start of the related action" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Concurrent With Start" .

action-relationship-type:concurrent-with-end
        a           owl:Class ;
        rdfs:label  "Concurrent With End" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The action must be performed concurrent with the end of the related action" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Concurrent With End" .

action-relationship-type:concurrent
        a           owl:Class ;
        rdfs:label  "Concurrent" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The action must be performed concurrent with the related action" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Concurrent" .

action-relationship-type:before
        a           owl:Class ;
        rdfs:label  "Before" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The action must be performed before the related action" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Before" .
