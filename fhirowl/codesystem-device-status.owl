@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix device-status: <http://hl7.org/fhir/device-status/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

device-status:unknown
        a           owl:Class ;
        rdfs:label  "Unknown" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The status of the device has not been determined." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Unknown" .

device-status:  a        owl:Ontology ;
        rdfs:comment     "The availability status of the device." ;
        rdfs:label       "FHIRDeviceStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/device-status/3.1.0" ;
        owl:versionInfo  "FHIRDeviceStatus(3.1.0)" .

device-status:active  a  owl:Class ;
        rdfs:label  "Active" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The Device is available for use.  Note: This means for *implanted devices*  the device is implanted in the patient." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Active" .

device-status:inactive
        a           owl:Class ;
        rdfs:label  "Inactive" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The Device is no longer available for use (e.g. lost, expired, damaged).  Note: This means for *implanted devices*  the device has been removed from the patient." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Inactive" .

device-status:entered-in-error
        a           owl:Class ;
        rdfs:label  "Entered in Error" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The Device was entered in error and voided." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Entered in Error" .
