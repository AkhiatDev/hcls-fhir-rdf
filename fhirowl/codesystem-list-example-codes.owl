@prefix list-example-codes: <http://hl7.org/fhir/list-example-use-codes/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

list-example-codes:plans
        a           owl:Class ;
        rdfs:label  "Care Plans" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A set of care plans that apply in a particular context of care." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Care Plans" .

list-example-codes:  a   owl:Ontology ;
        rdfs:comment     "Example use codes for the List resource - typical kinds of use." ;
        rdfs:label       "Example Use Codes for List" ;
        owl:versionIRI   "http://hl7.org/fhir/list-example-use-codes/3.1.0" ;
        owl:versionInfo  "Example Use Codes for List(3.1.0)" .

list-example-codes:allergies
        a           owl:Class ;
        rdfs:label  "Allergies" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A list of Allergies for the patient." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Allergies" .

list-example-codes:protocols
        a           owl:Class ;
        rdfs:label  "Protocols" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A set of protocols to be followed." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Protocols" .

list-example-codes:adverserxns
        a           owl:Class ;
        rdfs:label  "Adverse Reactions" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A list of part adverse reactions." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Adverse Reactions" .

list-example-codes:problems
        a           owl:Class ;
        rdfs:label  "Problem List" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A list of problems that the patient is known of have (or have had in the past)." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Problem List" .

list-example-codes:alerts
        a           owl:Class ;
        rdfs:label  "Alerts" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A list of alerts for the patient." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Alerts" .

list-example-codes:medications
        a           owl:Class ;
        rdfs:label  "Medication List" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A list of medication statements for the patient." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Medication List" .

list-example-codes:worklist
        a           owl:Class ;
        rdfs:label  "Worklist" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A list of items that constitute a set of work to be performed (typically this code would be specialized for more specific uses, such as a ward round list)." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Worklist" .

list-example-codes:waiting
        a           owl:Class ;
        rdfs:label  "Waiting List" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A list of items waiting for an event (perhaps a surgical patient waiting list)." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Waiting List" .
