@prefix encounter-admit-source: <http://hl7.org/fhir/admit-source/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

encounter-admit-source:mp
        a           owl:Class ;
        rdfs:label  "Medical Practitioner/physician referral" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient has been admitted due to a referred from a Specialist (as opposed to a General Practitioner)." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Medical Practitioner/physician referral" .

encounter-admit-source:outp
        a           owl:Class ;
        rdfs:label  "From outpatient department" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient has been transferred from an outpatient department within the hospital." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "From outpatient department" .

encounter-admit-source:rehab
        a           owl:Class ;
        rdfs:label  "From rehabilitation facility" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient has been transferred from a rehabilitiation facility or clinic." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "From rehabilitation facility" .

encounter-admit-source:hosp-trans
        a           owl:Class ;
        rdfs:label  "Transferred from other hospital" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The Patient has been transferred from another hospital for this encounter." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Transferred from other hospital" .

encounter-admit-source:other
        a           owl:Class ;
        rdfs:label  "Other" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient has been admitted from a source otherwise not specified here." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Other" .

encounter-admit-source:born
        a           owl:Class ;
        rdfs:label  "Born in hospital" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient is a newborn and the encounter will track the baby related activities (as opposed to the Mothers encounter - that may be associated using the newborn encounters partof property)" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Born in hospital" .

encounter-admit-source:gp
        a           owl:Class ;
        rdfs:label  "General Practitioner referral" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient has been admitted due to a referred from a General Practitioner." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "General Practitioner referral" .

encounter-admit-source:emd
        a           owl:Class ;
        rdfs:label  "From accident/emergency department" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient has been transferred from the emergency department within the hospital. This is typically used in the transition to an inpatient encounter" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "From accident/emergency department" .

encounter-admit-source:psych
        a           owl:Class ;
        rdfs:label  "From psychiatric hospital" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient has been transferred from a psychiatric facility." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "From psychiatric hospital" .

encounter-admit-source:
        a                owl:Ontology ;
        rdfs:comment     "This value set defines a set of codes that can be used to indicate from where the patient came in." ;
        rdfs:label       "AdmitSource" ;
        owl:versionIRI   "http://hl7.org/fhir/admit-source/3.1.0" ;
        owl:versionInfo  "AdmitSource(3.1.0)" .

encounter-admit-source:nursing
        a           owl:Class ;
        rdfs:label  "From nursing home" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient has been transferred from a nursing home." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "From nursing home" .
