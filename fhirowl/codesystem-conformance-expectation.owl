@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix conformance-expectation: <http://hl7.org/fhir/conformance-expectation/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

conformance-expectation:SHOULD-NOT
        a           owl:Class ;
        rdfs:label  "SHOULD-NOT" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Support for the specified capability is strongly discouraged and should occur only after careful consideration." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "SHOULD-NOT" .

conformance-expectation:
        a                owl:Ontology ;
        rdfs:comment     "Indicates the degree of adherence to a specified behavior or capability expected for a system to be deemed conformant with a specification." ;
        rdfs:label       "ConformanceExpectation" ;
        owl:versionIRI   "http://hl7.org/fhir/conformance-expectation/3.1.0" ;
        owl:versionInfo  "ConformanceExpectation(3.1.0)" .

conformance-expectation:MAY
        a           owl:Class ;
        rdfs:label  "MAY" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Support for the specified capability is not necessary to be considered conformant, and the requirement should be considered strictly optional." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "MAY" .

conformance-expectation:SHOULD
        a           owl:Class ;
        rdfs:label  "SHOULD" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Support for the specified capability is strongly encouraged, and failure to support it should only occur after careful consideration." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "SHOULD" .

conformance-expectation:SHALL
        a           owl:Class ;
        rdfs:label  "SHALL" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Support for the specified capability is required to be considered conformant." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "SHALL" .
