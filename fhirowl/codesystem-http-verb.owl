@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix http-verb: <http://hl7.org/fhir/http-verb/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

http-verb:GET  a    owl:Class ;
        rdfs:label  "GET" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "HTTP GET" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "GET" .

http-verb:DELETE  a  owl:Class ;
        rdfs:label  "DELETE" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "HTTP DELETE" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "DELETE" .

http-verb:  a            owl:Ontology ;
        rdfs:comment     "HTTP verbs (in the HTTP command line)." ;
        rdfs:label       "HTTPVerb" ;
        owl:versionIRI   "http://hl7.org/fhir/http-verb/3.1.0" ;
        owl:versionInfo  "HTTPVerb(3.1.0)" .

http-verb:POST  a   owl:Class ;
        rdfs:label  "POST" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "HTTP POST" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "POST" .

http-verb:PUT  a    owl:Class ;
        rdfs:label  "PUT" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "HTTP PUT" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "PUT" .
