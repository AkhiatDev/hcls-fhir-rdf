@prefix w5:    <http://hl7.org/fhir/w5#> .

contactentity-type:PRESS
        a           owl:Class ;
        rdfs:label  "Press" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Dedicated contact point for matters relating to press enquiries." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Press" .

contactentity-type:  a   owl:Ontology ;
        rdfs:comment     "This example value set defines a set of codes that can be used to indicate the purpose for which you would contact a contact party." ;
        rdfs:label       "ContactEntityType" ;
        owl:versionIRI   "http://hl7.org/fhir/contactentity-type/3.1.0" ;
        owl:versionInfo  "ContactEntityType(3.1.0)" .

contactentity-type:ADMIN
        a           owl:Class ;
        rdfs:label  "Administrative" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Contact details for administrative enquiries." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Administrative" .

contactentity-type:PAYOR
        a           owl:Class ;
        rdfs:label  "Payor" ;
        <http://www.w3.org/2004/02/skos/core#definition>
