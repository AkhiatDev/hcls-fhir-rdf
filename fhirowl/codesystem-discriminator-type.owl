@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix discriminator-type: <http://hl7.org/fhir/discriminator-type/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

discriminator-type:  a   owl:Ontology ;
        rdfs:comment     "How an element value is interpreted when discrimination is evaluated" ;
        rdfs:label       "DiscriminatorType" ;
        owl:versionIRI   "http://hl7.org/fhir/discriminator-type/3.1.0" ;
        owl:versionInfo  "DiscriminatorType(3.1.0)" .

discriminator-type:value
        a           owl:Class ;
        rdfs:label  "Value" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The slices have different values in the nominated element" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Value" .

discriminator-type:exists
        a           owl:Class ;
        rdfs:label  "Exists" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The slices are differentiated by the presence or absence of the nominated element" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Exists" .

discriminator-type:type
        a           owl:Class ;
        rdfs:label  "Type" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The slices are differentiated by type of the nominated element to a specifed profile" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Type" .

discriminator-type:profile
        a           owl:Class ;
        rdfs:label  "Profile" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The slices are differentiated by conformance of the nominated element to a specifed profile" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Profile" .

discriminator-type:pattern
        a           owl:Class ;
        rdfs:label  "Pattern" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The slices have different values in the nominated element, as determined by testing them against the applicable ElementDefinition.pattern[x]" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Pattern" .
