@prefix fhir:  <http://hl7.org/fhir/> .
@prefix map-transform: <http://hl7.org/fhir/map-transform/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

map-transform:cast  a  owl:Class ;
        rdfs:label  "cast" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "cast(source, type?) - case source from one type to another. target type can be left as implicit if there is one and only one target type known" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "cast" .

map-transform:create  a  owl:Class ;
        rdfs:label  "create" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "create(type : string) - type is passed through to the application on the standard API, and must be known by it" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "create" .

map-transform:  a        owl:Ontology ;
        rdfs:comment     "How data is copied/created" ;
        rdfs:label       "StructureMapTransform" ;
        owl:versionIRI   "http://hl7.org/fhir/map-transform/3.1.0" ;
        owl:versionInfo  "StructureMapTransform(3.1.0)" .

map-transform:cp  a  owl:Class ;
        rdfs:label  "cp" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Create a contact details. Parameters = (value) or (system, value). If no system is provided, the system should be inferred from the content of the value" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "cp" .

map-transform:pointer
        a           owl:Class ;
        rdfs:label  "pointer" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Return the appropriate string to put in a reference that refers to the resource provided as a parameter" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "pointer" .

map-transform:cc  a  owl:Class ;
        rdfs:label  "cc" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Create a CodeableConcept. Parameters = (text) or (system. Code[, display])" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "cc" .

map-transform:reference
        a           owl:Class ;
        rdfs:label  "reference" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "reference(source : object) - return a string that references the provided tree properly" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "reference" .

map-transform:escape  a  owl:Class ;
        rdfs:label  "escape" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "escape(source, fmt1, fmt2) - change source from one kind of escaping to another (plain, java, xml, json). note that this is for when the string itself is escaped" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "escape" .

map-transform:copy  a  owl:Class ;
        rdfs:label  "copy" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "copy(source)" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "copy" .

map-transform:c  a  owl:Class ;
        rdfs:label  "c" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Create a Coding. Parameters = (system. Code[, display])" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "c" .

map-transform:id  a  owl:Class ;
        rdfs:label  "id" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Create an identifier. Parameters = (system, value[, type]) where type is a code from the identifier type value set" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "id" .

map-transform:evaluate
        a           owl:Class ;
        rdfs:label  "evaluate" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Execute the supplied fluentpath expression and use the value returned by that" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "evaluate" .

map-transform:qty  a  owl:Class ;
        rdfs:label  "qty" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Create a quantity. Parameters = (text) or (value, unit, [system, code]) where text is the natural representation e.g. [comparator]value[space]unit" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "qty" .

map-transform:truncate
        a           owl:Class ;
        rdfs:label  "truncate" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "truncate(source, length) - source must be stringy type" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "truncate" .

map-transform:translate
        a           owl:Class ;
        rdfs:label  "translate" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "translate(source, uri_of_map) - use the translate operation" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "translate" .

map-transform:dateOp  a  owl:Class ;
        rdfs:label  "dateOp" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Perform a date operation. *Parameters to be documented*" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "dateOp" .

map-transform:append  a  owl:Class ;
        rdfs:label  "append" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "append(source...) - source is element or string" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "append" .

map-transform:uuid  a  owl:Class ;
        rdfs:label  "uuid" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Generate a random UUID (in lowercase). No Parameters" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "uuid" .
