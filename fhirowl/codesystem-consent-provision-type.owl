@prefix consent-provision-type: <http://hl7.org/fhir/consent-provision-type/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

consent-provision-type:deny
        a           owl:Class ;
        rdfs:label  "Opt Out" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Consent is denied for actions meeting these rules" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Opt Out" .

consent-provision-type:
        a                owl:Ontology ;
        rdfs:comment     "How a rule statement is applied, such as adding additional consent or removing consent" ;
        rdfs:label       "ConsentProvisionType" ;
        owl:versionIRI   "http://hl7.org/fhir/consent-provision-type/3.1.0" ;
        owl:versionInfo  "ConsentProvisionType(3.1.0)" .

consent-provision-type:permit
        a           owl:Class ;
        rdfs:label  "Opt In" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Consent is provided for actions meeting these rules" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Opt In" .
