@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix address-use: <http://hl7.org/fhir/address-use/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

address-use:old  a  owl:Class ;
        rdfs:label  "Old / Incorrect" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "This address is no longer in use (or was never correct, but retained for records)." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Old / Incorrect" .

address-use:  a          owl:Ontology ;
        rdfs:comment     "The use of an address" ;
        rdfs:label       "AddressUse" ;
        owl:versionIRI   "http://hl7.org/fhir/address-use/3.1.0" ;
        owl:versionInfo  "AddressUse(3.1.0)" .

address-use:home  a  owl:Class ;
        rdfs:label  "Home" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A communication address at a home." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Home" .

address-use:temp  a  owl:Class ;
        rdfs:label  "Temporary" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A temporary address. The period can provide more detailed information." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Temporary" .

address-use:work  a  owl:Class ;
        rdfs:label  "Work" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "An office address. First choice for business related contacts during business hours." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Work" .
