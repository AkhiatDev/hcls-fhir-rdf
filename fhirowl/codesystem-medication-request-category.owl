@prefix medication-request-category: <http://hl7.org/fhir/medication-request-category/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

medication-request-category:outpatient
        a           owl:Class ;
        rdfs:label  "Outpatient" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Includes orders for medications to be administered or consumed in an outpatient setting (for example, Emergency Department, Outpatient Clinic, Outpatient Surgery, Doctor's office)" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Outpatient" .

medication-request-category:inpatient
        a           owl:Class ;
        rdfs:label  "Inpatient" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Includes orders for medications to be administered or consumed in an inpatient or acute care setting" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Inpatient" .

medication-request-category:community
        a           owl:Class ;
        rdfs:label  "Community" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Includes orders for medications to be administered or consumed by the patient in their home (this would include long term care or nursing homes, hospices, etc)" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Community" .

medication-request-category:
        a                owl:Ontology ;
        rdfs:comment     "A coded concept identifying where the medication ordered is expected to be consumed or administered" ;
        rdfs:label       "MedicationRequestCategory" ;
        owl:versionIRI   "http://hl7.org/fhir/medication-request-category/3.1.0" ;
        owl:versionInfo  "MedicationRequestCategory(3.1.0)" .
