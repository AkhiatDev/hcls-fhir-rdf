@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix flag-category: <http://hl7.org/fhir/flag-category/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

flag-category:  a        owl:Ontology ;
        rdfs:comment     "Example list of general categories for flagged issues. (Not complete or necessarily appropriate.)" ;
        rdfs:label       "Flag Category" ;
        owl:versionIRI   "http://hl7.org/fhir/flag-category/3.1.0" ;
        owl:versionInfo  "Flag Category(3.1.0)" .

flag-category:lab  a  owl:Class ;
        rdfs:label  "Lab" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Flags related to performing laboratory tests and related processes (e.g. phlebotomy)." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Lab" .

flag-category:admin  a  owl:Class ;
        rdfs:label  "Administrative" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Flags related to administrative and financial processes." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Administrative" .

flag-category:diet  a  owl:Class ;
        rdfs:label  "Diet" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Flags related to the subject's dietary needs." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Diet" .

flag-category:contact
        a           owl:Class ;
        rdfs:label  "Subject contact" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Flags related to coming into contact with the patient." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Subject contact" .

flag-category:drug  a  owl:Class ;
        rdfs:label  "Drug" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Flags related to the patient's medications." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Drug" .
