@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix task-status: <http://hl7.org/fhir/task-status/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

task-status:failed  a  owl:Class ;
        rdfs:label  "Failed" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The task was attempted but could not be completed due to some error." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Failed" .

task-status:entered-in-error
        a           owl:Class ;
        rdfs:label  "Entered in Error" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The task should never have existed and is retained only because of the possibility it may have used." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Entered in Error" .

task-status:requested
        a           owl:Class ;
        rdfs:label  "Requested" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The task is ready to be acted upon and action is sought." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Requested" .

task-status:in-progress
        a           owl:Class ;
        rdfs:label  "In Progress" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Task has been started but is not yet complete." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "In Progress" .

task-status:rejected  a  owl:Class ;
        rdfs:label  "Rejected" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The potential performer who claimed ownership of the task has decided not to execute it prior to performing any action." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Rejected" .

task-status:received  a  owl:Class ;
        rdfs:label  "Received" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A potential performer has claimed ownership of the task and is evaluating whether to perform it." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Received" .

task-status:accepted  a  owl:Class ;
        rdfs:label  "Accepted" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The potential performer has agreed to execute the task but has not yet started work." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Accepted" .

task-status:draft  a  owl:Class ;
        rdfs:label  "Draft" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The task is not yet ready to be acted upon." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Draft" .

task-status:  a          owl:Ontology ;
        rdfs:comment     "The current status of the task." ;
        rdfs:label       "TaskStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/task-status/3.1.0" ;
        owl:versionInfo  "TaskStatus(3.1.0)" .

task-status:on-hold  a  owl:Class ;
        rdfs:label  "On Hold" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Task has been started but work has been paused." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "On Hold" .

task-status:ready  a  owl:Class ;
        rdfs:label  "Ready" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Task is ready to be performed, but no action has yet been taken.  Used in place of requested/received/accepted/rejected when request assignment and acceptance is a given." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Ready" .

task-status:completed
        a           owl:Class ;
        rdfs:label  "Completed" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The task has been completed." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Completed" .

task-status:cancelled
        a           owl:Class ;
        rdfs:label  "Cancelled" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The task was not completed." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Cancelled" .
