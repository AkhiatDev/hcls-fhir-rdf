@prefix v3-MaritalStatus: <http://hl7.org/fhir/v3/MaritalStatus/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-MaritalStatus:T  a  owl:Class ;
        rdfs:label  "Domestic partner" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Person declares that a domestic partner relationship exists." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Domestic partner" .

v3-MaritalStatus:  a     owl:Ontology ;
        rdfs:comment     " * * * No description supplied * * *  Open Issue:\r\nThe specific meanings of these codes can vary somewhat by jurisdiction and implementation so caution should be used when determining equivalency." ;
        rdfs:label       "v3 Code System MaritalStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/MaritalStatus/2016-11-11" ;
        owl:versionInfo  "v3 Code System MaritalStatus(2016-11-11)" .

v3-MaritalStatus:M  a  owl:Class ;
        rdfs:label  "Married" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A current marriage contract is active" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Married" .

v3-MaritalStatus:W  a  owl:Class ;
        rdfs:label  "Widowed" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The spouse has died" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Widowed" .

v3-MaritalStatus:A  a  owl:Class ;
        rdfs:label  "Annulled" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Marriage contract has been declared null and to not have existed" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Annulled" .

v3-MaritalStatus:P  a  owl:Class ;
        rdfs:label  "Polygamous" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "More than 1 current spouse" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Polygamous" .

v3-MaritalStatus:U  a  owl:Class ;
        rdfs:label  "unmarried" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Currently not in a marriage contract." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "unmarried" .

v3-MaritalStatus:D  a  owl:Class ;
        rdfs:label  "Divorced" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Marriage contract has been declared dissolved and inactive" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Divorced" .

v3-MaritalStatus:I  a  owl:Class ;
        rdfs:label  "Interlocutory" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Subject to an Interlocutory Decree." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Interlocutory" .

v3-MaritalStatus:S  a  owl:Class ;
        rdfs:label  "Never Married" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "No marriage contract has ever been entered" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Never Married" .

v3-MaritalStatus:L  a  owl:Class ;
        rdfs:label  "Legally Separated" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Legally Separated" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Legally Separated" .
