@prefix v3-DeviceAlertLevel: <http://hl7.org/fhir/v3/DeviceAlertLevel/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-DeviceAlertLevel:C
        a           owl:Class ;
        rdfs:label  "Critical" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Shut Down, Fix Problem and Re-initialize" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Critical" .

v3-DeviceAlertLevel:W
        a           owl:Class ;
        rdfs:label  "Warning" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Corrective Action Anticipated" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Warning" .

v3-DeviceAlertLevel:  a  owl:Ontology ;
        rdfs:comment     " Domain values for the Device.Alert_levelCode" ;
        rdfs:label       "v3 Code System DeviceAlertLevel" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/DeviceAlertLevel/2016-11-11" ;
        owl:versionInfo  "v3 Code System DeviceAlertLevel(2016-11-11)" .

v3-DeviceAlertLevel:N
        a           owl:Class ;
        rdfs:label  "Normal" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "No Corrective Action Needed" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Normal" .

v3-DeviceAlertLevel:S
        a           owl:Class ;
        rdfs:label  "Serious" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Corrective Action Required" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Serious" .
