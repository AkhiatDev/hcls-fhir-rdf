@prefix v3-PaymentTerms: <http://hl7.org/fhir/v3/PaymentTerms/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-PaymentTerms:N60  a  owl:Class ;
        rdfs:label  "Net 60 days" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Payment in full for products and/or services is required 60 days from the time the service is performed or goods delivered." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Net 60 days" .

v3-PaymentTerms:N30  a  owl:Class ;
        rdfs:label  "Net 30 days" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Payment in full for products and/or services is required 30 days from the time the service is performed or goods delivered." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Net 30 days" .

v3-PaymentTerms:  a      owl:Ontology ;
        rdfs:comment     " Describes payment terms for a financial transaction, used in an invoice. This is typically expressed as a responsibility of the acceptor or payor of an invoice." ;
        rdfs:label       "v3 Code System PaymentTerms" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/PaymentTerms/2016-11-11" ;
        owl:versionInfo  "v3 Code System PaymentTerms(2016-11-11)" .

v3-PaymentTerms:N90  a  owl:Class ;
        rdfs:label  "Net 90 days" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Payment in full for products and/or services is required 90 days from the time the service is performed or goods delivered." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Net 90 days" .

v3-PaymentTerms:COD  a  owl:Class ;
        rdfs:label  "Cash on Delivery" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Payment in full for products and/or services is required as soon as the service is performed or goods delivered." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Cash on Delivery" .
