@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix v3-EntityClass: <http://hl7.org/fhir/v3/EntityClass/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-EntityClass:PLC  a    owl:Class ;
        rdfs:label       "place" ;
        rdfs:subClassOf  v3-EntityClass:ENT ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A physical place or site with its containing structure. May be natural or man-made. The geographic position of a place may or may not be constant." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "place" .

v3-EntityClass:HOLD  a   owl:Class ;
        rdfs:label       "holder" ;
        rdfs:subClassOf  v3-EntityClass:CONT ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A type of container that can hold other containers or other holders." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "holder" .

v3-EntityClass:RGRP  a   owl:Class ;
        rdfs:label       "group" ;
        rdfs:subClassOf  v3-EntityClass:ENT ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A grouping of resources (personnel, material, or places) to be used for scheduling purposes.  May be a pool of like-type resources, a team, or combination of personnel, material and places." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "group" .

v3-EntityClass:CER  a    owl:Class ;
        rdfs:label       "certificate representation" ;
        rdfs:subClassOf  v3-EntityClass:DEV ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A physical artifact that stores information about the granting of authorization." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "certificate representation" .

v3-EntityClass:PLNT  a   owl:Class ;
        rdfs:label       "plant" ;
        rdfs:subClassOf  v3-EntityClass:NLIV ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A living subject from the order of plants." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "plant" .

v3-EntityClass:CITY  a   owl:Class ;
        rdfs:label       "city or town" ;
        rdfs:subClassOf  v3-EntityClass:PLC ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The territory of a city, town or other municipality." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "city or town" .

v3-EntityClass:MMAT  a   owl:Class ;
        rdfs:label       "manufactured material" ;
        rdfs:subClassOf  v3-EntityClass:MAT ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Corresponds to the ManufacturedMaterial class" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "manufactured material" .

v3-EntityClass:PUB  a    owl:Class ;
        rdfs:label       "public institution" ;
        rdfs:subClassOf  v3-EntityClass:ORG ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "An agency of the people of a state often assuming some authority over a certain matter. Includes government, governmental agencies, associations." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "public institution" .

v3-EntityClass:ANM  a    owl:Class ;
        rdfs:label       "animal" ;
        rdfs:subClassOf  v3-EntityClass:NLIV ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A living subject from the animal kingdom." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "animal" .

v3-EntityClass:DEV  a    owl:Class ;
        rdfs:label       "device" ;
        rdfs:subClassOf  v3-EntityClass:MMAT ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A subtype of ManufacturedMaterial used in an activity, without being substantially changed through that activity.  The kind of device is identified by the code attribute inherited from Entity.\r\n\n                        \n                           Usage: This includes durable (reusable) medical equipment as well as disposable equipment." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "device" .

v3-EntityClass:COUNTY
        a                owl:Class ;
        rdfs:label       "county or parish" ;
        rdfs:subClassOf  v3-EntityClass:PLC ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The territory of a county, parish or other division of a state or province." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "county or parish" .

v3-EntityClass:FOOD  a   owl:Class ;
        rdfs:label       "food" ;
        rdfs:subClassOf  v3-EntityClass:MAT ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Naturally occurring, processed or manufactured entities that are primarily used as food for humans and animals." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "food" .

v3-EntityClass:PROVINCE
        a                owl:Class ;
        rdfs:label       "state or province" ;
        rdfs:subClassOf  v3-EntityClass:PLC ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The territory of a state, province, department or other division of a sovereign country." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "state or province" .

v3-EntityClass:  a       owl:Ontology ;
        rdfs:comment     " Classifies the Entity class and all of its subclasses.  The terminology is hierarchical.  At the top is this  HL7-defined domain of high-level categories (such as represented by the Entity subclasses). Each of these terms must be harmonized and is specializable. The value sets beneath are drawn from multiple, frequently external, domains that reflect much more fine-grained typing." ;
        rdfs:label       "v3 Code System EntityClass" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/EntityClass/2016-11-11" ;
        owl:versionInfo  "v3 Code System EntityClass(2016-11-11)" .

v3-EntityClass:PSN  a    owl:Class ;
        rdfs:label       "person" ;
        rdfs:subClassOf  v3-EntityClass:LIV ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A living subject of the species homo sapiens." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "person" .

v3-EntityClass:STATE  a  owl:Class ;
        rdfs:label       "state" ;
        rdfs:subClassOf  v3-EntityClass:ORG ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A politically organized body of people bonded by territory, culture, or ethnicity, having sovereignty (to a certain extent) granted by other states (enclosing or neighboring states). This includes countries (nations), provinces (e.g., one of the United States of America or a French departement), counties or municipalities. Refine using, e.g., ISO country codes, FIPS-PUB state codes, etc." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "state" .

v3-EntityClass:MODDV  a  owl:Class ;
        rdfs:label       "imaging modality" ;
        rdfs:subClassOf  v3-EntityClass:DEV ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Class to contain unique attributes of diagnostic imaging equipment." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "imaging modality" .

v3-EntityClass:ENT  a  owl:Class ;
        rdfs:label  "entity" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Corresponds to the Entity class" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "entity" .

v3-EntityClass:NAT  a    owl:Class ;
        rdfs:label       "Nation" ;
        rdfs:subClassOf  v3-EntityClass:STATE ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A politically organized body of people bonded by territory and known as a nation." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Nation" .

v3-EntityClass:HCE  a    owl:Class ;
        rdfs:label       "health chart entity" ;
        rdfs:subClassOf  v3-EntityClass:ENT ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A health chart included to serve as a document receiving entity in the management of medical records." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "health chart entity" .

v3-EntityClass:MAT  a    owl:Class ;
        rdfs:label       "material" ;
        rdfs:subClassOf  v3-EntityClass:ENT ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Any thing that has extension in space and mass, may be of living or non-living origin." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "material" .

v3-EntityClass:MIC  a    owl:Class ;
        rdfs:label       "microorganism" ;
        rdfs:subClassOf  v3-EntityClass:NLIV ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "All single celled living organisms including protozoa, bacteria, yeast, viruses, etc." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "microorganism" .

v3-EntityClass:LIV  a    owl:Class ;
        rdfs:label       "living subject" ;
        rdfs:subClassOf  v3-EntityClass:ENT ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Anything that essentially has the property of life, independent of current state (a dead human corpse is still essentially a living subject)." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "living subject" .

v3-EntityClass:CONT  a   owl:Class ;
        rdfs:label       "container" ;
        rdfs:subClassOf  v3-EntityClass:MMAT ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A container of other entities." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "container" .

v3-EntityClass:COUNTRY
        a                owl:Class ;
        rdfs:label       "country" ;
        rdfs:subClassOf  v3-EntityClass:PLC ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The territory of a sovereign nation." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "country" .

v3-EntityClass:CHEM  a   owl:Class ;
        rdfs:label       "chemical substance" ;
        rdfs:subClassOf  v3-EntityClass:MAT ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A substance that is fully defined by an organic or inorganic chemical formula, includes mixtures of other chemical substances. Refine using, e.g., IUPAC codes." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "chemical substance" .

v3-EntityClass:NLIV  a   owl:Class ;
        rdfs:label       "non-person living subject" ;
        rdfs:subClassOf  v3-EntityClass:LIV ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A subtype of living subject that includes all living things except the species Homo Sapiens." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "non-person living subject" .

v3-EntityClass:ORG  a    owl:Class ;
        rdfs:label       "organization" ;
        rdfs:subClassOf  v3-EntityClass:ENT ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A social or legal structure formed by human beings." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "organization" .
