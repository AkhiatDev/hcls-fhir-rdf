@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix v3-EquipmentAlertLevel: <http://hl7.org/fhir/v3/EquipmentAlertLevel/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

v3-EquipmentAlertLevel:N
        a           owl:Class ;
        rdfs:label  "Normal" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "No Corrective Action Needed" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Normal" .

v3-EquipmentAlertLevel:S
        a           owl:Class ;
        rdfs:label  "Serious" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Corrective Action Required" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Serious" .

v3-EquipmentAlertLevel:C
        a           owl:Class ;
        rdfs:label  "Critical" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Shut Down, Fix Problem and Re-init" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Critical" .

v3-EquipmentAlertLevel:
        a                owl:Ontology ;
        rdfs:comment     "**** MISSING DEFINITIONS ****" ;
        rdfs:label       "v3 Code System EquipmentAlertLevel" ;
        owl:versionIRI   "http://hl7.org/fhir/v3/EquipmentAlertLevel/2016-11-11" ;
        owl:versionInfo  "v3 Code System EquipmentAlertLevel(2016-11-11)" .

v3-EquipmentAlertLevel:W
        a           owl:Class ;
        rdfs:label  "Warning" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Corrective Action Anticipated" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Warning" .
