@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix goal-relationship-type: <http://hl7.org/fhir/goal-relationship-type/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

goal-relationship-type:replacement
        a           owl:Class ;
        rdfs:label  "Replacement" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Indicates that this goal has been replaced by the target goal" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Replacement" .

goal-relationship-type:milestone
        a           owl:Class ;
        rdfs:label  "Milestone" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Indicates that the target goal is considered to be a \"piece\" of attaining this goal." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Milestone" .

goal-relationship-type:predecessor
        a           owl:Class ;
        rdfs:label  "Predecessor" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Indicates that the target goal is one which must be met before striving for the current goal" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Predecessor" .

goal-relationship-type:successor
        a           owl:Class ;
        rdfs:label  "Successor" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Indicates that the target goal is a desired objective once the current goal is met" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Successor" .

goal-relationship-type:other
        a           owl:Class ;
        rdfs:label  "Other" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Indicates that the relationship is not covered by one of the pre-defined codes.  (An extension may convey more information about the meaning of the relationship.)" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Other" .

goal-relationship-type:
        a                owl:Ontology ;
        rdfs:comment     "Types of relationships between two goals" ;
        rdfs:label       "GoalRelationshipType" ;
        owl:versionIRI   "http://hl7.org/fhir/goal-relationship-type/3.1.0" ;
        owl:versionInfo  "GoalRelationshipType(3.1.0)" .
