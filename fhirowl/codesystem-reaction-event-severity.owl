@prefix reaction-event-severity: <http://hl7.org/fhir/reaction-event-severity/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

reaction-event-severity:mild
        a           owl:Class ;
        rdfs:label  "Mild" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Causes mild physiological effects." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Mild" .

reaction-event-severity:severe
        a           owl:Class ;
        rdfs:label  "Severe" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Causes severe physiological effects." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Severe" .

reaction-event-severity:
        a                owl:Ontology ;
        rdfs:comment     "Clinical assessment of the severity of a reaction event as a whole, potentially considering multiple different manifestations." ;
        rdfs:label       "AllergyIntoleranceSeverity" ;
        owl:versionIRI   "http://hl7.org/fhir/reaction-event-severity/3.1.0" ;
        owl:versionInfo  "AllergyIntoleranceSeverity(3.1.0)" .

reaction-event-severity:moderate
        a           owl:Class ;
        rdfs:label  "Moderate" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Causes moderate physiological effects." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Moderate" .
