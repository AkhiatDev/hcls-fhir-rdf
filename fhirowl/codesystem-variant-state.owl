@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix variant-state: <http://hl7.org/fhir/variant-state/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

variant-state:positive
        a           owl:Class ;
        rdfs:label  "positive" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "the variant is detected" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "positive" .

variant-state:  a        owl:Ontology ;
        rdfs:comment     "Codes providing the status of the variant test result" ;
        rdfs:label       "sequenceStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/variant-state/3.1.0" ;
        owl:versionInfo  "sequenceStatus(3.1.0)" .

variant-state:negative
        a           owl:Class ;
        rdfs:label  "negative" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "no variant is detected" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "negative" .

variant-state:absent  a  owl:Class ;
        rdfs:label  "absent" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "result of the variant is missing" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "absent" .
