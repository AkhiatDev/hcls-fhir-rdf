@prefix map-input-mode: <http://hl7.org/fhir/map-input-mode/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

map-input-mode:  a       owl:Ontology ;
        rdfs:comment     "Mode for this instance of data" ;
        rdfs:label       "StructureMapInputMode" ;
        owl:versionIRI   "http://hl7.org/fhir/map-input-mode/3.1.0" ;
        owl:versionInfo  "StructureMapInputMode(3.1.0)" .

map-input-mode:target
        a           owl:Class ;
        rdfs:label  "Target Instance" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Names an instance that is being populated" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Target Instance" .

map-input-mode:source
        a           owl:Class ;
        rdfs:label  "Source Instance" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Names an input instance used a source for mapping" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Source Instance" .
