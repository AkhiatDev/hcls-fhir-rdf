@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix content-type: <http://hl7.org/fhir/content-type/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

content-type:xml  a  owl:Class ;
        rdfs:label  "xml" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "XML content-type corresponding to the application/fhir+xml mime-type." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "xml" .

content-type:json  a  owl:Class ;
        rdfs:label  "json" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "JSON content-type corresponding to the application/fhir+json mime-type." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "json" .

content-type:none  a  owl:Class ;
        rdfs:label  "none" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Prevent the use of the corresponding http header." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "none" .

content-type:ttl  a  owl:Class ;
        rdfs:label  "ttl" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "RDF content-type corresponding to the text/turtle mime-type." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "ttl" .

content-type:  a         owl:Ontology ;
        rdfs:comment     "The content or mime type." ;
        rdfs:label       "ContentType" ;
        owl:versionIRI   "http://hl7.org/fhir/content-type/3.1.0" ;
        owl:versionInfo  "ContentType(3.1.0)" .
