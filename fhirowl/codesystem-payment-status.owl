@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix payment-status: <http://hl7.org/fhir/paymentstatus/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

payment-status:  a       owl:Ontology ;
        rdfs:comment     "This value set includes a sample set of Payment Status codes." ;
        rdfs:label       "Payment Status Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/paymentstatus/3.1.0" ;
        owl:versionInfo  "Payment Status Codes(3.1.0)" .

payment-status:cleared
        a           owl:Class ;
        rdfs:label  "Cleared" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The payment has been received by the payee." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Cleared" .

payment-status:paid  a  owl:Class ;
        rdfs:label  "Paid" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The payment has been sent physically or electronically." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Paid" .
