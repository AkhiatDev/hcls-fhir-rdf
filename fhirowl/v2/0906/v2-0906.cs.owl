v2-0906:CSP  a      owl:Class ;
        rdfs:label  "Callback placer for scheduling - filler should contact the placer to schedule the service" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Callback placer for scheduling - filler should contact the placer to schedule the service" .

v2-0906:CS  a       owl:Class ;
