        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Callback for scheduling - Filler should contact the placer (or target) to schedule the service." .

v2-0906:T  a        owl:Class ;
        rdfs:label  "Timing critical - It is critical to come as close as possible to the requested time (e.g. for a through antimicrobial level)." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Timing critical - It is critical to come as close as possible to the requested time (e.g. for a through antimicrobial level)." .

v2-0906:RR  a       owl:Class ;
        rdfs:label  "Rush reporting - A report should be prepared and sent as quickly as possible" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Rush reporting - A report should be prepared and sent as quickly as possible" .

v2-0906:UD  a       owl:Class ;
        rdfs:label  "Use as directed - Drug is to be used as directed by the prescriber." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Use as directed - Drug is to be used as directed by the prescriber." .

v2-0906:R  a        owl:Class ;
        rdfs:label  "Routine - Routine service, do at usual work hours" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Routine - Routine service, do at usual work hours" .

v2-0906:PRN  a      owl:Class ;
