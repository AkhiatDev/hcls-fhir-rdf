@prefix location-status: <http://hl7.org/fhir/location-status/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

location-status:inactive
        a           owl:Class ;
        rdfs:label  "Inactive" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The location is no longer used." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Inactive" .

location-status:suspended
        a           owl:Class ;
        rdfs:label  "Suspended" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The location is temporarily closed." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Suspended" .

location-status:active
        a           owl:Class ;
        rdfs:label  "Active" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The location is operational." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Active" .

location-status:  a      owl:Ontology ;
        rdfs:comment     "Indicates whether the location is still in use." ;
        rdfs:label       "LocationStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/location-status/3.1.0" ;
        owl:versionInfo  "LocationStatus(3.1.0)" .
