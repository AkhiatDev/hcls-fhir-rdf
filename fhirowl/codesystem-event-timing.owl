@prefix event-timing: <http://hl7.org/fhir/event-timing/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

event-timing:PHS  a  owl:Class ;
        rdfs:label  "After Sleep" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "event occurs [offset] after subject goes to sleep" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "After Sleep" .

event-timing:AFT  a  owl:Class ;
        rdfs:label  "Afternoon" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "event occurs during the afternoon" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Afternoon" .

event-timing:NIGHT  a  owl:Class ;
        rdfs:label  "Night" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "event occurs during the night" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Night" .

event-timing:MORN  a  owl:Class ;
        rdfs:label  "Morning" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "event occurs during the morning" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Morning" .

event-timing:  a         owl:Ontology ;
        rdfs:comment     "Real world event relating to the schedule." ;
        rdfs:label       "EventTiming" ;
        owl:versionIRI   "http://hl7.org/fhir/event-timing/3.1.0" ;
        owl:versionInfo  "EventTiming(3.1.0)" .

event-timing:EVE  a  owl:Class ;
        rdfs:label  "Evening" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "event occurs during the evening" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Evening" .
