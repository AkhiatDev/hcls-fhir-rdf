@prefix encounter-discharge-disposition: <http://hl7.org/fhir/discharge-disposition/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

encounter-discharge-disposition:aadvice
        a           owl:Class ;
        rdfs:label  "Left against advice" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient self discharged against medical advice." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Left against advice" .

encounter-discharge-disposition:exp
        a           owl:Class ;
        rdfs:label  "Expired" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient has deceased during this encounter." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Expired" .

encounter-discharge-disposition:rehab
        a           owl:Class ;
        rdfs:label  "Rehabilitation" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient was discharged and is to receive post acute care rehabilitation services." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Rehabilitation" .

encounter-discharge-disposition:hosp
        a           owl:Class ;
        rdfs:label  "Hospice" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient has been discharged into palliative care." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Hospice" .

encounter-discharge-disposition:home
        a           owl:Class ;
        rdfs:label  "Home" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient was dicharged and has indicated that they are going to return home afterwards." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Home" .

encounter-discharge-disposition:
        a                owl:Ontology ;
        rdfs:comment     "This value set defines a set of codes that can be used to where the patient left the hospital." ;
        rdfs:label       "DischargeDisposition" ;
        owl:versionIRI   "http://hl7.org/fhir/discharge-disposition/3.1.0" ;
        owl:versionInfo  "DischargeDisposition(3.1.0)" .

encounter-discharge-disposition:psy
        a           owl:Class ;
        rdfs:label  "Psychiatric hospital" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient has been transferred to a psychiatric facility." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Psychiatric hospital" .

encounter-discharge-disposition:oth
        a           owl:Class ;
        rdfs:label  "Other" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The discharge disposition has not otherwise defined." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Other" .

encounter-discharge-disposition:snf
        a           owl:Class ;
        rdfs:label  "Skilled nursing facility" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient has been discharged to a skilled nursing facility for the patient to receive additional care." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Skilled nursing facility" .

encounter-discharge-disposition:long
        a           owl:Class ;
        rdfs:label  "Long-term care" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient has been discharged into long-term care where is likely to be monitored through an ongoing episode-of-care." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Long-term care" .

encounter-discharge-disposition:other-hcf
        a           owl:Class ;
        rdfs:label  "Other healthcare facility" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient was transferred to another healthcare facility." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Other healthcare facility" .
