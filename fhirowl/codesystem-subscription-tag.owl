@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix subscription-tag: <http://hl7.org/fhir/subscription-tag/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

subscription-tag:queued
        a           owl:Class ;
        rdfs:label  "Queued" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The message has been queued for processing on a destination systems." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Queued" .

subscription-tag:  a     owl:Ontology ;
        rdfs:comment     "Tags to put on a resource after subscriptions have been sent." ;
        rdfs:label       "SubscriptionTag" ;
        owl:versionIRI   "http://hl7.org/fhir/subscription-tag/3.1.0" ;
        owl:versionInfo  "SubscriptionTag(3.1.0)" .

subscription-tag:delivered
        a           owl:Class ;
        rdfs:label  "Delivered" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The message has been delivered to its intended recipient." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Delivered" .
