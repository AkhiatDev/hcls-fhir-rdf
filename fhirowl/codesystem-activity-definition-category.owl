@prefix activity-definition-category: <http://hl7.org/fhir/activity-definition-category/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

activity-definition-category:treatment
        a           owl:Class ;
        rdfs:label  "Treatment" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The activity is intended to provide or is related to treatment of the patient" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Treatment" .

activity-definition-category:assessment
        a           owl:Class ;
        rdfs:label  "Assessment" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The activity is intended to perform or is related to assessment of the patient" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Assessment" .

activity-definition-category:education
        a           owl:Class ;
        rdfs:label  "Education" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The activity is intended to provide or is related to education of the patient" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Education" .

activity-definition-category:
        a                owl:Ontology ;
        rdfs:comment     "High-level categorization of the type of activity" ;
        rdfs:label       "ActivityDefinitionCategory" ;
        owl:versionIRI   "http://hl7.org/fhir/activity-definition-category/3.1.0" ;
        owl:versionInfo  "ActivityDefinitionCategory(3.1.0)" .
