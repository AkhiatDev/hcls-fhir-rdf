@prefix surface: <http://hl7.org/fhir/FDI-surface/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

surface:D  a        owl:Class ;
        rdfs:label  "Distal" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The surface of a tooth that faces away from the midline of the face." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Distal" .

surface:I  a        owl:Class ;
        rdfs:label  "Incisal" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The biting edge of anterior teeth." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Incisal" .

surface:B  a        owl:Class ;
        rdfs:label  "Buccal" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The surface of a posterior tooth facing the cheeks." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Buccal" .

surface:DO  a       owl:Class ;
        rdfs:label  "Distoclusal" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The Distoclusal surfaces of a tooth." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Distoclusal" .

surface:L  a        owl:Class ;
        rdfs:label  "Lingual" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The surface of a tooth facing the tongue." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Lingual" .

surface:V  a        owl:Class ;
        rdfs:label  "Ventral" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The surface of a tooth facing the lips." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Ventral" .

surface:MO  a       owl:Class ;
        rdfs:label  "Mesioclusal" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The Mesioclusal surfaces of a tooth." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Mesioclusal" .

surface:O  a        owl:Class ;
        rdfs:label  "Occlusal" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The chewing surface of posterior teeth." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Occlusal" .

surface:MOD  a      owl:Class ;
        rdfs:label  "Mesioclusodistal" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The Mesioclusodistal surfaces of a tooth." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Mesioclusodistal" .

surface:  a              owl:Ontology ;
        rdfs:comment     "This value set includes a smattering of FDI tooth surface codes." ;
        rdfs:label       "Surface Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/FDI-surface/3.1.0" ;
        owl:versionInfo  "Surface Codes(3.1.0)" .

surface:M  a        owl:Class ;
        rdfs:label  "Mesial" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The surface of a tooth that is closest to the midline (middle) of the face." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Mesial" .

surface:DI  a       owl:Class ;
        rdfs:label  "Distoincisal" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The Distoincisal surfaces of a tooth." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Distoincisal" .
