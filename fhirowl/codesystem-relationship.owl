@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix relationship: <http://hl7.org/fhir/relationship/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

relationship:4  a   owl:Class ;
        rdfs:label  "Common Law Spouse" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient is the common law spouse of the subscriber (policy holder)" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Common Law Spouse" .

relationship:2  a   owl:Class ;
        rdfs:label  "Spouse" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient is the spouse or equivalent of the subscriber (policy holder)" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Spouse" .

relationship:5  a   owl:Class ;
        rdfs:label  "Other" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient has some other relationship, such as parent, to the subscriber (policy holder)" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Other" .

relationship:3  a   owl:Class ;
        rdfs:label  "Child" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient is the child of the subscriber (policy holder)" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Child" .

relationship:1  a   owl:Class ;
        rdfs:label  "Self" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The patient is the subscriber (policy holder)" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Self" .

relationship:  a         owl:Ontology ;
        rdfs:comment     "This value set includes the Patient to subscriber relationship codes." ;
        rdfs:label       "Beneficiary Relationship Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/relationship/3.1.0" ;
        owl:versionInfo  "Beneficiary Relationship Codes(3.1.0)" .
