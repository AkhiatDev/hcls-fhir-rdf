@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix audit-event-action: <http://hl7.org/fhir/audit-event-action/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

audit-event-action:D  a  owl:Class ;
        rdfs:label  "Delete" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Delete items, such as a doctor master file record." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Delete" .

audit-event-action:  a   owl:Ontology ;
        rdfs:comment     "Indicator for type of action performed during the event that generated the event" ;
        rdfs:label       "AuditEventAction" ;
        owl:versionIRI   "http://hl7.org/fhir/audit-event-action/3.1.0" ;
        owl:versionInfo  "AuditEventAction(3.1.0)" .

audit-event-action:E  a  owl:Class ;
        rdfs:label  "Execute" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Perform a system or application function such as log-on, program execution or use of an object's method, or perform a query/search operation." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Execute" .

audit-event-action:C  a  owl:Class ;
        rdfs:label  "Create" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Create a new database object, such as placing an order." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Create" .

audit-event-action:R  a  owl:Class ;
        rdfs:label  "Read/View/Print" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Display or print data, such as a doctor census." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Read/View/Print" .

audit-event-action:U  a  owl:Class ;
        rdfs:label  "Update" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Update data, such as revise patient information." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Update" .
