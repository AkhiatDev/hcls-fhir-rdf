@prefix immunization-origin: <http://hl7.org/fhir/immunization-origin/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

immunization-origin:  a  owl:Ontology ;
        rdfs:comment     "The value set to instantiate this attribute should be drawn from a terminologically robust code system that consists of or contains concepts to support describing the source of the data when the report of the immunization event is not based on information from the person, entity or organization who administered the vaccine. This value set is provided as a suggestive example." ;
        rdfs:label       "Immunization Origin Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/immunization-origin/3.1.0" ;
        owl:versionInfo  "Immunization Origin Codes(3.1.0)" .

immunization-origin:record
        a           owl:Class ;
        rdfs:label  "Written Record" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The data for the immunization event originated with a written record for the patient." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Written Record" .

immunization-origin:school
        a           owl:Class ;
        rdfs:label  "School Record" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The data for the immunization event originated with a school record for the patient." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "School Record" .

immunization-origin:provider
        a           owl:Class ;
        rdfs:label  "Other Provider" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The data for the immunization event originated with another provider." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Other Provider" .

immunization-origin:recall
        a           owl:Class ;
        rdfs:label  "Parent/Guardian/Patient Recall" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The data for the immunization event originated from the recollection of the patient or parent/guardian of the patient." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Parent/Guardian/Patient Recall" .
