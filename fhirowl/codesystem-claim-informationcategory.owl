@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix claim-informationcategory: <http://hl7.org/fhir/claiminformationcategory/> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

claim-informationcategory:related
        a           owl:Class ;
        rdfs:label  "Related Services" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Nature and date of the related event eg. Last exam, service, Xray etc." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Related Services" .

claim-informationcategory:onset
        a           owl:Class ;
        rdfs:label  "Onset" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Period, start or end dates of aspects of the Condition." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Onset" .

claim-informationcategory:material
        a           owl:Class ;
        rdfs:label  "Materials Forwarded" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Materials being forwarded, eg. Models, molds, images, documents." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Materials Forwarded" .

claim-informationcategory:prosthesis
        a           owl:Class ;
        rdfs:label  "Prosthesis" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The type of prosthesis and date of supply if a previously supplied prosthesis." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Prosthesis" .

claim-informationcategory:info
        a           owl:Class ;
        rdfs:label  "Information" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Codes conveying additional situation and condition information." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Information" .

claim-informationcategory:
        a                owl:Ontology ;
        rdfs:comment     "This value set includes sample Information Category codes." ;
        rdfs:label       "Claim Information Category Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/claiminformationcategory/3.1.0" ;
        owl:versionInfo  "Claim Information Category Codes(3.1.0)" .

claim-informationcategory:other
        a           owl:Class ;
        rdfs:label  "Other" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Other information identified by the type.system." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Other" .

claim-informationcategory:discharge
        a           owl:Class ;
        rdfs:label  "Discharge" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Discharge status and discharge to locations." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Discharge" .

claim-informationcategory:missingtooth
        a           owl:Class ;
        rdfs:label  "Missing Tooth" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Teeth which are missing for any reason, for example: prior extraction, never developed." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Missing Tooth" .

claim-informationcategory:exception
        a           owl:Class ;
        rdfs:label  "Exception" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Insurance policy exceptions." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Exception" .

claim-informationcategory:attachment
        a           owl:Class ;
        rdfs:label  "Attachment" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Materials attached such as images, documents and resources." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Attachment" .
