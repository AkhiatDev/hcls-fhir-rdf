@prefix risk-probability: <http://hl7.org/fhir/risk-probability/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

risk-probability:  a     owl:Ontology ;
        rdfs:comment     "Codes representing the likelihood of a particular outcome in a risk assessment." ;
        rdfs:label       "Risk Probability" ;
        owl:versionIRI   "http://hl7.org/fhir/risk-probability/3.1.0" ;
        owl:versionInfo  "Risk Probability(3.1.0)" .

risk-probability:negligible
        a           owl:Class ;
        rdfs:label  "Negligible likelihood" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The specified outcome is exceptionally unlikely." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Negligible likelihood" .

risk-probability:certain
        a           owl:Class ;
        rdfs:label  "Certain" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The specified outcome is effectively guaranteed." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Certain" .

risk-probability:low  a  owl:Class ;
        rdfs:label  "Low likelihood" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The specified outcome is possible but unlikely." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Low likelihood" .

risk-probability:high
        a           owl:Class ;
        rdfs:label  "High likelihood" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The specified outcome is more likely to occur than not." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "High likelihood" .

risk-probability:moderate
        a           owl:Class ;
        rdfs:label  "Moderate likelihood" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The specified outcome has a reasonable likelihood of occurrence." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Moderate likelihood" .
