        owl:versionIRI   "http://hl7.org/fhir/resource-status/null" ;
        owl:versionInfo  "Canonical Status Codes for FHIR Resources(null)" .

resource-status:active
        a           owl:Class ;
        rdfs:label  "active" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The resource describes information that is currently valid or a process that is presently occuring" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
