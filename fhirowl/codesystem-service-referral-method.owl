@prefix service-referral-method: <http://hl7.org/fhir/service-referral-method/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

service-referral-method:
        a                owl:Ontology ;
        rdfs:comment     "The methods of referral can be used when referring to a specific HealthCareService resource." ;
        rdfs:label       "ReferralMethod" ;
        owl:versionIRI   "http://hl7.org/fhir/service-referral-method/3.1.0" ;
        owl:versionInfo  "ReferralMethod(3.1.0)" .

service-referral-method:fax
        a           owl:Class ;
        rdfs:label  "Fax" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Referrals may be accepted by fax." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Fax" .

service-referral-method:mail
        a           owl:Class ;
        rdfs:label  "Mail" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Referrals may be accepted via regular postage (or hand delivered)." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Mail" .

service-referral-method:phone
        a           owl:Class ;
        rdfs:label  "Phone" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Referrals may be accepted over the phone from a practitioner." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Phone" .

service-referral-method:semail
        a           owl:Class ;
        rdfs:label  "Secure Email" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Referrals may be accepted via a secure email. To send please encrypt with the services public key." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Secure Email" .

service-referral-method:elec
        a           owl:Class ;
        rdfs:label  "Secure Messaging" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Referrals may be accepted via a secure messaging system. To determine the types of secure messaging systems supported, refer to the identifiers collection. Callers will need to understand the specific identifier system used to know that they are able to transmit messages." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Secure Messaging" .
