@prefix message-transport: <http://hl7.org/fhir/message-transport/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

message-transport:ftp
        a           owl:Class ;
        rdfs:label  "FTP" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The application sends or receives messages using File Transfer Protocol." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "FTP" .

message-transport:  a    owl:Ontology ;
        rdfs:comment     "The protocol used for message transport." ;
        rdfs:label       "MessageTransport" ;
        owl:versionIRI   "http://hl7.org/fhir/message-transport/3.1.0" ;
        owl:versionInfo  "MessageTransport(3.1.0)" .

message-transport:http
        a           owl:Class ;
        rdfs:label  "HTTP" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The application sends or receives messages using HTTP POST (may be over http: or https:)." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "HTTP" .

message-transport:mllp
        a           owl:Class ;
        rdfs:label  "MLLP" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The application sends or receives messages using HL7's Minimal Lower Level Protocol." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "MLLP" .
