@prefix special-values: <http://hl7.org/fhir/special-values/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

special-values:trace  a  owl:Class ;
        rdfs:label  "Trace Amount Detected" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The content is greater than zero, but too small to be quantified." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Trace Amount Detected" .

special-values:withdrawn
        a           owl:Class ;
        rdfs:label  "Value Withdrawn" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The value is no longer available." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Value Withdrawn" .

special-values:false  a  owl:Class ;
        rdfs:label  "false" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Boolean false." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "false" .

special-values:sufficient
        a           owl:Class ;
        rdfs:label  "Sufficient Quantity" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The specific quantity is not known, but is known to be non-zero and is not specified because it makes up the bulk of the material." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Sufficient Quantity" .

special-values:nil-known
        a           owl:Class ;
        rdfs:label  "Nil Known" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The are no known applicable values in this context." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Nil Known" .

special-values:true  a  owl:Class ;
        rdfs:label  "true" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Boolean true." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "true" .

special-values:  a       owl:Ontology ;
        rdfs:comment     "A set of generally useful codes defined so they can be included in value sets." ;
        rdfs:label       "SpecialValues" ;
        owl:versionIRI   "http://hl7.org/fhir/special-values/3.1.0" ;
        owl:versionInfo  "SpecialValues(3.1.0)" .
