@prefix service-pharmacy: <http://hl7.org/fhir/ex-pharmaservice/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

service-pharmacy:compoundfee
        a           owl:Class ;
        rdfs:label  "Compounding Fee" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The professional fee charged for compounding the medication." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Compounding Fee" .

service-pharmacy:drugcost
        a           owl:Class ;
        rdfs:label  "Drug Cost" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The wholesale price of the medication." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Drug Cost" .

service-pharmacy:dispensefee
        a           owl:Class ;
        rdfs:label  "Dispense Fee" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The professional fee charged for dispensing the product or service." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Dispense Fee" .

service-pharmacy:flushot
        a           owl:Class ;
        rdfs:label  "Flu Shot" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Flu Shot" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Flu Shot" .

service-pharmacy:smokecess
        a           owl:Class ;
        rdfs:label  "Smoking cessation" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Smoking cessation" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Smoking cessation" .

service-pharmacy:  a     owl:Ontology ;
        rdfs:comment     "This value set includes a smattering of Pharmacy Service codes." ;
        rdfs:label       "Example Pharmacy Service Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/ex-pharmaservice/3.1.0" ;
        owl:versionInfo  "Example Pharmacy Service Codes(3.1.0)" .

service-pharmacy:markup
        a           owl:Class ;
        rdfs:label  "Markup" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The additional cost assessed on the drug." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Markup" .
