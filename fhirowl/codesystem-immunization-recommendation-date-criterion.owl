@prefix immunization-recommendation-date-criterion: <http://hl7.org/fhir/immunization-recommendation-date-criterion/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

immunization-recommendation-date-criterion:latest
        a           owl:Class ;
        rdfs:label  "Latest" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The latest date the next dose is to be given." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Latest" .

immunization-recommendation-date-criterion:earliest
        a           owl:Class ;
        rdfs:label  "Earliest Date" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "As early as possible." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Earliest Date" .

immunization-recommendation-date-criterion:overdue
        a           owl:Class ;
        rdfs:label  "Past Due Date" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Date the next dose is considered overdue." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Past Due Date" .

immunization-recommendation-date-criterion:due
        a           owl:Class ;
        rdfs:label  "Due" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Date the next dose is considered due." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Due" .

immunization-recommendation-date-criterion:
        a                owl:Ontology ;
        rdfs:comment     "The value set to instantiate this attribute should be drawn from a terminologically robust code system that consists of or contains concepts to support the definition of dates relevant to recommendations for future doses of vaccines. This value set is provided as a suggestive example." ;
        rdfs:label       "Immunization Recommendation Date Criterion Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/immunization-recommendation-date-criterion/3.1.0" ;
        owl:versionInfo  "Immunization Recommendation Date Criterion Codes(3.1.0)" .

immunization-recommendation-date-criterion:recommended
        a           owl:Class ;
        rdfs:label  "Recommended" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "At the recommended date." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Recommended" .
