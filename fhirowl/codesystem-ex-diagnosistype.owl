@prefix ex-diagnosistype: <http://hl7.org/fhir/ex-diagnosistype/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

ex-diagnosistype:retrospective
        a           owl:Class ;
        rdfs:label  "Retrospective Diagnosis" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The labeling of an illness in a specific historical event using modern knowledge, methods and disease classifications." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Retrospective Diagnosis" .

ex-diagnosistype:admitting
        a           owl:Class ;
        rdfs:label  "Admitting Diagnosis" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The diagnosis given as the reason why the patient was admitted to the hospital." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Admitting Diagnosis" .

ex-diagnosistype:self
        a           owl:Class ;
        rdfs:label  "Self Diagnosis" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A diagnosis determined by the patient." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Self Diagnosis" .

ex-diagnosistype:nursing
        a           owl:Class ;
        rdfs:label  "Nursing Diagnosis" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A diagnosis which identifies people's responses to situations in their lives, such as a readiness to change or a willingness to accept assistance." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Nursing Diagnosis" .

ex-diagnosistype:prenatal
        a           owl:Class ;
        rdfs:label  "Prenatal Diagnosis" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A diagnosis determined prior to birth." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Prenatal Diagnosis" .

ex-diagnosistype:laboratory
        a           owl:Class ;
        rdfs:label  "Laboratory Diagnosis" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A diagnosis based significantly on laboratory reports or test results, rather than the physical examination of the patient." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Laboratory Diagnosis" .

ex-diagnosistype:remote
        a           owl:Class ;
        rdfs:label  "Remote Diagnosis" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A diagnosis determined using telemedicine techniques." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Remote Diagnosis" .

ex-diagnosistype:principal
        a           owl:Class ;
        rdfs:label  "Principal Diagnosis" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The single medical diagnosis that is most relevant to the patient's chief complaint or need for treatment." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Principal Diagnosis" .

ex-diagnosistype:clinical
        a           owl:Class ;
        rdfs:label  "Clinical Diagnosis" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A diagnosis made on the basis of medical signs and patient-reported symptoms, rather than diagnostic tests." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Clinical Diagnosis" .

ex-diagnosistype:  a     owl:Ontology ;
        rdfs:comment     "This value set includes example Diagnosis Type codes." ;
        rdfs:label       "Example Diagnosis Type Codes" ;
        owl:versionIRI   "http://hl7.org/fhir/ex-diagnosistype/3.1.0" ;
        owl:versionInfo  "Example Diagnosis Type Codes(3.1.0)" .

ex-diagnosistype:discharge
        a           owl:Class ;
        rdfs:label  "Discharge Diagnosis" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The diagnosis given when the patient is discharged from the hospital." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Discharge Diagnosis" .

ex-diagnosistype:radiology
        a           owl:Class ;
        rdfs:label  "Radiology Diagnosis" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "A diagnosis based primarily on the results from medical imaging studies." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Radiology Diagnosis" .

ex-diagnosistype:differential
        a           owl:Class ;
        rdfs:label  "Differential Diagnosis" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "One of a set of the possible diagnoses that could be connected to the signs, symptoms, and lab findings." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Differential Diagnosis" .
