@prefix fhir:  <http://hl7.org/fhir/> .
@prefix substance-status: <http://hl7.org/fhir/substance-status/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

substance-status:active
        a           owl:Class ;
        rdfs:label  "Active" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The substance is considered for use or reference" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Active" .

substance-status:  a     owl:Ontology ;
        rdfs:comment     "A code to indicate if the substance is actively used" ;
        rdfs:label       "FHIRSubstanceStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/substance-status/3.1.0" ;
        owl:versionInfo  "FHIRSubstanceStatus(3.1.0)" .

substance-status:inactive
        a           owl:Class ;
        rdfs:label  "Inactive" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The substance is considered for reference, but not for use" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Inactive" .

substance-status:entered-in-error
        a           owl:Class ;
        rdfs:label  "Entered in Error" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The substance was entered in error" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Entered in Error" .
