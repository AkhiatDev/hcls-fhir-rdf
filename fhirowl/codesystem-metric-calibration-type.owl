@prefix metric-calibration-type: <http://hl7.org/fhir/metric-calibration-type/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

metric-calibration-type:two-point
        a           owl:Class ;
        rdfs:label  "Two Point" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Two-point metric calibration method" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Two Point" .

metric-calibration-type:unspecified
        a           owl:Class ;
        rdfs:label  "Unspecified" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Metric calibration method has not been identified." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Unspecified" .

metric-calibration-type:offset
        a           owl:Class ;
        rdfs:label  "Offset" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Offset metric calibration method" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Offset" .

metric-calibration-type:
        a                owl:Ontology ;
        rdfs:comment     "Describes the type of a metric calibration." ;
        rdfs:label       "DeviceMetricCalibrationType" ;
        owl:versionIRI   "http://hl7.org/fhir/metric-calibration-type/3.1.0" ;
        owl:versionInfo  "DeviceMetricCalibrationType(3.1.0)" .

metric-calibration-type:gain
        a           owl:Class ;
        rdfs:label  "Gain" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Gain metric calibration method" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Gain" .
