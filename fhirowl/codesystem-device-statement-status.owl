@prefix device-statement-status: <http://hl7.org/fhir/device-statement-status/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

device-statement-status:entered-in-error
        a           owl:Class ;
        rdfs:label  "Entered in Error" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The statement was recorded incorrectly." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Entered in Error" .

device-statement-status:on-hold
        a           owl:Class ;
        rdfs:label  "On Hold" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Actions implied by the statement have been temporarily halted, but are expected to continue later. May also be called \"suspended\"." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "On Hold" .

device-statement-status:stopped
        a           owl:Class ;
        rdfs:label  "Stopped" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "Actions implied by the statement have been permanently halted, before all of them occurred." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Stopped" .

device-statement-status:active
        a           owl:Class ;
        rdfs:label  "Active" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The device is still being used." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Active" .

device-statement-status:intended
        a           owl:Class ;
        rdfs:label  "Intended" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The device may be used at some time in the future." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Intended" .

device-statement-status:completed
        a           owl:Class ;
        rdfs:label  "Completed" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "The device is no longer being used." ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Completed" .

device-statement-status:
        a                owl:Ontology ;
        rdfs:comment     "A coded concept indicating the current status of a the Device Usage" ;
        rdfs:label       "DeviceUseStatementStatus" ;
        owl:versionIRI   "http://hl7.org/fhir/device-statement-status/3.1.0" ;
        owl:versionInfo  "DeviceUseStatementStatus(3.1.0)" .
