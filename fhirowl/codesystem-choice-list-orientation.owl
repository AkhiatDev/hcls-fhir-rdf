@prefix choice-list-orientation: <http://hl7.org/fhir/choice-list-orientation/> .
@prefix fhir:  <http://hl7.org/fhir/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix w5:    <http://hl7.org/fhir/w5#> .

choice-list-orientation:horizontal
        a           owl:Class ;
        rdfs:label  "Horizontal" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "List choices along the horizontal axis" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Horizontal" .

choice-list-orientation:
        a                owl:Ontology ;
        rdfs:comment     "Direction in which lists of question options should be displayed" ;
        rdfs:label       "ChoiceListOrientation" ;
        owl:versionIRI   "http://hl7.org/fhir/choice-list-orientation/3.1.0" ;
        owl:versionInfo  "ChoiceListOrientation(3.1.0)" .

choice-list-orientation:vertical
        a           owl:Class ;
        rdfs:label  "Vertical" ;
        <http://www.w3.org/2004/02/skos/core#definition>
                "List choices down the vertical axis" ;
        <http://www.w3.org/2004/02/skos/core#prefLabel>
                "Vertical" .
